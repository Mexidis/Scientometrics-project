@article{ WOS:000357500200012,
Author = {Castellanos, E. and Briscese, F. and Grether, M. and de Llano, M.},
Title = {Weakly interacting Bose-Einstein condensates in temperature-dependent
   generic traps},
Journal = {JETP LETTERS},
Year = {2015},
Volume = {101},
Number = {8},
Pages = {572-578},
Month = {APR},
Abstract = {We study the shift Delta T (c) in the condensation temperature of an
   atomic Bose-Einstein condensate trapped in a temperature-dependent
   three-dimensional generic potential. With no assumptions other than the
   mean-field approach and the semiclassical approximation, it is shown
   that the inclusion of a T-dependent trap improves upon the pure
