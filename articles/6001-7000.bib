Title = {Creating a Historical Database for Estimation Using the EPCU
   Approximation Approach for COSMIC (ISO 19761)},
Booktitle = {2016 FOURTH INTERNATIONAL CONFERENCE IN SOFTWARE ENGINEERING RESEARCH
   AND INNOVATION - CONISOFT 2016},
