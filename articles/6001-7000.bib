@article{ WOS:000390969200003,
Author = {Fuentes, B. E. and Lopez-Patino, J. and Yousif, F. B. and Martinez, H.},
Title = {Ionization and fragmentation of CH4 by proton impact},
Journal = {INTERNATIONAL JOURNAL OF MASS SPECTROMETRY},
Year = {2016},
Volume = {411},
Pages = {21-26},
Month = {DEC 10},
Abstract = {Total ionization and fragmentation cross sections for proton impact on
   CH4 within the energy range of 2-10 keV have been measured. Present
   results identify the ionization channel to be the dominant processes.
   Decay routs leading to the fragments CH3+, CH2+, CH+, C+ and H+, points
   to a decreasing cross sections relative to that of ionization. Present
   total cross section was found to agree with pervious experimental
   investigations. Structure within our present total cross sections was
   explained with respect to interference between competing different
   dissociating paths of fragments as well as transition between initial
   and final states as well as with scattering amplitude corresponding to
   different reaction pathways. Comparison with suggested theoretical model
   points to similar trends in the total absolute fragments cross sections.
