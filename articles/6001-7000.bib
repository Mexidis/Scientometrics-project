@article{ WOS:000346365900002,
Author = {Melendez, Josue},
Title = {Rigidity theorems for hypersurfaces with constant mean curvature},
Journal = {BULLETIN OF THE BRAZILIAN MATHEMATICAL SOCIETY},
Year = {2014},
Volume = {45},
Number = {3},
Pages = {385-404},
Month = {SEP},
Abstract = {Let M-n be a compact oriented hypersurface of a unit sphere Sn+1(1) with
