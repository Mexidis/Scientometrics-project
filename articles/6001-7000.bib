@article{ WOS:000358267300003,
Author = {Romero, M. and Escamilla, R. and Marquina, V. and Gomez, R.},
Title = {Structural and mechanic properties of RFeO3 with R = Y, Eu
   and La perovskites: a first-principles calculation},
Journal = {EUROPEAN PHYSICAL JOURNAL D},
Year = {2015},
Volume = {69},
Number = {7},
Month = {JUL 19},
Abstract = {The structural and elastic properties of the RFeO3 phases with R = Y,
   Eu, and La were investigated using first-principles plane-wave
   pseudopotential density functional theory with the generalized gradient
   approximation (GGA). The ground state properties (equilibrium cell
   constants) agree well with the reported experimental results. Our
   results showed an increase in the unit-cell volume (V) with the increase
   of ionic radii. We calculate a set of elastic parameters (bulk modulus
   B-VRH, shear modulus G(VRH), Young's modulus E-VRH and Poisson's ratio
   nu) in the framework of the Voigt-Reuss-Hill (VRH) approximation. The
   sound velocities (upsilon(l), upsilon(t)) and Debye temperature
   (theta(D)) were calculated using these elastic moduli. The calculated
   elastic constants were positives and satisfy the well-known Born
   criteria, indicating that the orthorhombic structure is stable. Finally,
   the ratio G(VRH)/B-VRH suggests that the RFeO3 phases are ductile in
   nature.},
