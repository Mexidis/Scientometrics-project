Address = {233 SPRING ST, NEW YORK, NY 10013 USA},
Type = {Article},
Language = {English},
Affiliation = {Craig, W (Corresponding Author), McMaster Univ, Dept Math \& Stat, Hamilton, ON L8S 4K1, Canada.
   Craig, Walter; Yang, Chi-Ru, McMaster Univ, Dept Math \& Stat, Hamilton, ON L8S 4K1, Canada.
   Garcia-Azpeitia, Carlos, Univ Nacl Autonoma Mexico, Fac Ciencias, Dept Matemat, Mexico City 04510, DF, Mexico.
