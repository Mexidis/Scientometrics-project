@article{ WOS:000346775600011,
Author = {Granados-Gonzalez, Gisela and Rheubert, Justin L. and
   Villagran-SantaCruz, Maricela and Gonzalez-Herrera, Maria E. and
   Davila-Cedillo, Jessica V. and Gribbins, Kevin M. and
   Hernandez-Gallegos, Oswald},
Title = {Male reproductive cycle in Aspidoscelis costata costata
   (Squamata: Teiidae) fro Tonatico, Estado de Mexico, Mexico},
Journal = {ACTA ZOOLOGICA},
Year = {2015},
Volume = {96},
Number = {1},
Pages = {108-116},
Month = {JAN},
Abstract = {The purpose of this study was to investigate the reproductive cycle of a
   highelevation population of Aspidoscelis costata costata (1500-1600 m)
   and compare its reproductive cycle with that of other populations,
   species, and closely related genera, Adult male A, costata costata
   lizards were collected, and the reproductive tracts were removed and
   subjected to histological analyses. Testicular activity commences in
   March with maximum testicular activity and highest sperm abundance (in
   the epididyrnides) occurring between May and July. The testis remains at
   peak activity until September,,vh en a late regression/ear1,7 quiescent
   phase is observed. Le,,,dig cells follow this same pattern except these
   hormone producing cells remain at maximum secretory level through
   September. Sperm are present in the epididymides in April September.
   This pattern is consistent with the spring recrudescence found in a
   multitude of male lizard taxa. However, this differs from the continuous
   cycle observed in some tropical Tend species and other lizard taxa at
   high elevation. This study indicates that our knowledge about lizard
   spen-natogenic cycles remains incomplete, and additional studies are
   required to fully understand the interactions between phenotype,
   evolution, phylogenetics and environment.},
