@article{ WOS:000386418200008,
Author = {Alvarez-Ospina, Harry and Peralta, Oscar and Castro, Telma and Saavedra,
   Maria I.},
Title = {Optimum quantification temperature for total, organic, and elemental
   carbon using thermal-coulombimetric analysis},
Journal = {ATMOSPHERIC ENVIRONMENT},
Year = {2016},
Volume = {145},
Pages = {74-80},
Month = {NOV},
Abstract = {The quantification of total (TC), organic (OC), elemental carbon (EC),
   and OC/TC, EC/TC ratios is useful to determine the original sources of
   carbonaceous particles, so the importance of using appropriate standards
   is often crucial in atmospheric sciences, and the National Institute of
   Standards and Technology (NIST) offers good analytes. The Standard
   Reference Material (SRM) Urban Particulate Matter has a similar matrix
   as in the airborne aerosols of urban atmospheres and a well-known
   composition. The optimum quantification temperature analysis for carbon
   content of SRM 1648a and 1649b using a coulometric method (CM5300 and
