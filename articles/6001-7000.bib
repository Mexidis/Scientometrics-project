@article{ WOS:000342693300023,
Author = {Salas, Alfredo and Diaz, Fernando and Denisse Re, Ana and
   Galindo-Sanchez, Clara E. and Sanchez-Castrejon, Edna and Gonzalez,
   Marco and Licea, Alexei and Sanchez-Zamora, Adolfo and Rosas, Carlos},
Title = {PREFERRED TEMPERATURE, THERMAL TOLERANCE, AND METABOLIC RESPONSE OF
   TEGULA REGINA (STEARNS, 1892)},
Journal = {JOURNAL OF SHELLFISH RESEARCH},
Year = {2014},
Volume = {33},
Number = {1},
Pages = {239-246},
Month = {APR},
Abstract = {The preferred temperature of Tegula regina was determined in a
   horizontal thermal gradient with organisms acclimated to 16 degrees C,
   19 degrees C, and 22 degrees C using to acutemethod, with a preferred
   temperature of 19.7 +/- 1.8 degrees C for the day cycle and 18.8 +/- 1.2
   degrees C for the night cycle. The final preferred temperature
   determined for marine snails was 19.2 +/- 1.5 degrees C. The
   displacement velocity decreased to an interval from 99.1 cm/h to 62.5
   cm/h. During the third hour, when organisms detected the preferred
   temperature, the velocity diminished gradually from 25.8 cm/h to 7.5
   cm/h. Critical temperature maxima (CTMax), which refers to the
   temperature at which at least 50\% of the experimental group displays a
   loss of attachment, was measured at three acclimation temperatures (16
   degrees C, 19 degrees C, and 22 degrees C). At the acclimation
   temperature (16 degrees C), 50\% of the experimental group had an
   attachment loss at a CTMax of 29.3 degrees C. At a warmer acclimation
   temperature (22 degrees C), the observed CTMax was 31.2 degrees C.
   Marine snail oxygen consumption rate increased significantly (P < 0.05,
   31\%) from 16 degrees C to 22 degrees C.},
