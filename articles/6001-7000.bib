@article{ WOS:000405824100001,
Author = {Gomez-Noguez, Felipe and Manuel Leon-Rossano, Luis and Mehltreter, Klaus
   and Orozco-Segovia, Alma and Rosas-Perez, Irma and Perez-Garcia, Blanca},
Title = {Experimental Measurements of Terminal Velocity of Fern Spores},
