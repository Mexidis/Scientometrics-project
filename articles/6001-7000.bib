@article{ WOS:000363803300004,
Author = {Montano-Rendon, Mauricio and Sanchez-Gonzalez, Luis A. and
   Hernandez-Alonso, German and Navarro-Sigueenza, Adolfo G.},
Title = {Genetic differentiation in the Mexican endemic Rufous-backed Robin,
   Turdus rufopalliatus (Passeriformes: Turdidae)},
Journal = {ZOOTAXA},
