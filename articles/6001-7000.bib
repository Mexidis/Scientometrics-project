@article{ WOS:000351058300002,
Author = {Padilla-Ramirez, Sergio and Diaz, Fernando and Denisse Re, Ana and
   Elizabeth Galindo-Sanchez, Clara and Lorena Sanchez-Lizarraga, Ana and
   Antonio Nunez-Moreno, Luis and Moreno-Sierra, Dalia and Paschke, Kurt
   and Rosas, Carlos},
Title = {The effects of thermal acclimation on the behavior, thermal tolerance,
   and respiratory metabolism in a crab inhabiting a wide range of thermal
   habitats (Cancer antennarius Stimpson, 1856, the red shore crab)},
Journal = {MARINE AND FRESHWATER BEHAVIOUR AND PHYSIOLOGY},
Year = {2015},
Volume = {48},
Number = {2},
Pages = {89-101},
Month = {MAR 4},
Abstract = {The preferred temperature of Cancer antennarius determined with the
   acute method was 15.2 degrees C, and with the gravitation method was
   15.9 +/- 1.9 in the day cycle and 13.2 +/- 0.6 degrees C in the night
   cycle. The critical thermal maximum increased as the acclimation
   temperature was increased (p<0.05); the thermal tolerance interval was
   1.3, indicating that this species has a low tolerance indicative that it
   has evolved in highly stable environments. The acclimation response
   ratio for adults had an interval of 0.10-0.30. These values allow us to
   characterize this species as inhabiting cold and temperate regions. The
   oxygen consumption rates increased significantly (p<0.05) from 6.57 to
   9.92.0mg O(2)kg(-1)h(-1) wet weight as the acclimation temperature
   increased from 15 to 24 degrees C. The range of temperature coefficient
   (Q(10)) between 15 and 18 degrees C was the lower than 1.70 and
   corresponds with the intervals of preferred temperature.},
