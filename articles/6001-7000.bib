@article{ WOS:000379227800017,
Author = {Nieves-Uribe, Sandra and Castro-Gerardino, Jimena and Flores-Gallardo,
   Adrian and Llorente-Bousquets, Jorge},
Title = {Chorion in the Genera Anteos and Rhabdodryas1: its
   Significance and Implications},
Journal = {SOUTHWESTERN ENTOMOLOGIST},
Year = {2016},
Volume = {41},
Number = {2},
Pages = {485-504},
Month = {JUN},
Abstract = {We studied and illustrated the structure of the chorion of four species
   from two genera of the subfamily Coliadinae: Anteos and Rhabdodryas. The
   descriptions of Anteos maerula and A. clorinde were reviewed with new
   materials from Mexico and Colombia. The chorionic relief and the
   micro-grid of A. menippe were inspected for the first time, in addition
   to the Rhabdodryas trite, thought absent. Using a scanning electron
   microscope (SEM), photographs were taken of four chorionic regions
   (micropylar, perimicropylar or apical, equator, and base) in the three
   species of Anteos; these were described, illustrated, and compared. The
   images in SEM allowed analysis and determining that the micro-grid is an
   external section of exochorion (relief): multiple polygonal
   micro-reticular matrix in each unit. Until now their presence in the
   genera studied and Prestonia, Aphrissa, and Phoebis allowed postulation
   of it as a synapomorphy in the group called `American Catopsilias'.
   Clear and distinguishably the micro-grid, as described here, is only
   registered with this clade of Pieridae, Coliadinae. The characters
   specialized in the chorion recorded in A. menippe allowed us to suggest
   that the genus Rhodocera for this species is synonymous of Anteos.},
