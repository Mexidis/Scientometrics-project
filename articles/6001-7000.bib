@article{ WOS:000379634000019,
Author = {Arocha, Jorge L. and Montejano, Amanda},
Title = {Null and non-rainbow colorings of projective plane and sphere
   triangulations},
Journal = {DISCRETE APPLIED MATHEMATICS},
Year = {2016},
