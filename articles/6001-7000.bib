@article{ WOS:000364245000009,
Author = {Merino, Bernardo Gonzalez and Jonard-Perez, Natalia},
Title = {A pseudometric invariant under similarities in the hyperspace of
   non-degenerated compact convex sets of Rn},
Journal = {TOPOLOGY AND ITS APPLICATIONS},
Year = {2015},
Volume = {194},
Pages = {125-143},
Month = {OCT},
Abstract = {In this work we define a new pseudometric in K-{*}(n), the hyperspace of
   all non-degenerated compact convex sets of]R-n, which is invariant under
   similarities. We will prove that the quotient space generated by this
   pseudometric (which is the orbit space generated by the natural action
   of the group of similarities on K-{*}(n)) is homeomorphic to the
   Banach-Mazur compactum BM(n), while K-{*}(n) is homeomorphic to the
   topological product Q X Rn+1, where Q stands for the Hilbert cube.
   Finally we will show some consequences in convex geometry, namely, we
   measure how much two convex bodies differ (by means of our new
   pseudometric) in terms of some classical functionals. (C) 2015 Elsevier
   B.V. All rights reserved.},
