@article{ WOS:000384291700013,
Author = {Nieves-Uribe, Sandra and Castro-Gerardino, Jimena and Flores-Gallardo,
   Adrian and Llorente-Bousquets, Jorge},
Title = {Chorionic Micro-rid in the Genera Nathalis Boisduval and
   Kricogonia Reakirt: Taxonomic Implications and Evolutive Trends},
Journal = {SOUTHWESTERN ENTOMOLOGIST},
Year = {2016},
Volume = {41},
Number = {3},
Pages = {715-733},
Month = {SEP},
Abstract = {We describe and compare the chorionic architecture of three species,
   belonging to less derivative Coliadinae genera: Nathalis plauta
   (Doubleday, 1847), N. i. iole (Boisduval, 1836), and Kricogonia lyside
   (Godart, 1819). Such selection constitutes a refinement from previous
   studies of the chorion within this group, recognizing the presence of
   microreticulum in all three species. Scanning electron microscopy (SEM)
   is incorporated with the previous staining technique, enriching the
   description and analysis of the chorionic architecture in the apical,
   equatorial, and basal regions. We typify two kinds of hexagonal
   macroreticulum and two kinds of rectangular macroreticulum; an
   intermediate state between the two is identified by the concept of
   `zig-zagging' axis, with taxonomic and phylogenetic implications. The
   overall results have implications for systematics of Coliadinae with
   aspects in classification and biogeography.},
