@article{ WOS:000405527600002,
Author = {Villanueva-Viramontes, Sara and Hernandez-Apolinar, Mariana and
   Carnevali Fernandez-Concha, German and Dorantes-Euan, Alfredo and Dzib,
   Gabriel R. and Martinez-Castillo, Jaime},
Title = {Wild Vanilla planifolia and its relatives in the Mexican Yucatan
   Peninsula: Systematic analyses with ISSR and ITS},
Journal = {BOTANICAL SCIENCES},
Year = {2017},
Volume = {95},
Number = {2},
Pages = {169-187},
Month = {APR-JUN},
Abstract = {Background: Vanilla planifolia, a threatened species, is distributed
   naturally in semi-deciduous and evergreen rain forests of southeastern
   Mexico and parts of Central America. In the wild, it is difficult to
   diagnose from another sympatric Vanilla species, and individuals with
   reproductive structures are usually required.
   Questions: Can ISSR discriminate wild individuals of Vanilla planifolia
   from other sympatric species of the genus? Can phylogenetic analyses of
   ITS recover the monophyly of V. planifolia and help identify Vanilla
   species?
   Studied species: The vanilla V. planifolia), the second-most important
   economically spice in the world.
   Methods: We analyzed 88 wild individuals of several Vanilla species.
   Individuals of five species of Vanilla reported for the region were used
   as reference. 84 ISSR loci were analyzed using three clustering methods.
   A phylogenetic inference using ITS was performed.
   Results: ISSR markers clearly discriminate wild Vanilla planifolia,
   finding definite genetic structure within the species. The three
   clustering methods identified genetic relationships with congruent
   patterns. Five groups were found and they corresponded with the species
   studied. Phylogenetic inference of ITS sequences supported the monophyly
   of the Vanilla and the resulting cladograms were coherent with the
   clustering pattern observed in the ISSR studies.
   Conclusions: Both ISSR and ITS analyses are able to identify V.
   planifolia. Molecular data suggest the presence of V. pompona and a new
   species of Vanilla in the Yucatan Peninsula.},
Publisher = {SOC BOTANICA MEXICO},
Address = {APARTADO POSTAL 70-385  DELEGACION COYOACAN, CIUDAD UNIV, MEXICO, D F
   00000, MEXICO},
Type = {Article},
Language = {English},
Affiliation = {Villanueva-Viramontes, S; Fernández-Concha, GC; Dorantes-Euán, A; Dzib, GR; Martínez-Castillo, J (Corresponding Author), Ctr Invest Cient Yucatan AC, Merida, Yucatan, Mexico.
   Hernández-Apolinar, M (Corresponding Author), UNAM Cd, Fac Ciencias, Dept Ecol \& Recursos Nat, Mexico City, DF, Mexico.
