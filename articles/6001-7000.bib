@article{ WOS:000346792400002,
Author = {Cristadoro, Giampaolo and Gilbert, Thomas and Lenci, Marco and Sanders,
   David P.},
Title = {Transport properties of Levy walks: An analysis in terms of multistate
   processes},
Journal = {EPL},
Year = {2014},
Volume = {108},
Number = {5},
Month = {DEC},
Abstract = {Continuous time random walks combining diffusive and ballistic regimes
   are introduced to describe a class of Levy walks on lattices. By
   including exponentially distributed waiting times separating the
   successive jump events of a walker, we are led to a description of such
   Levy walks in terms of multistate processes whose time-evolution is
   shown to obey a set of coupled delay differential equations. Using
   simple arguments, we obtain asymptotic solutions to these equations and
   rederive the scaling laws for the mean squared displacement of such
   processes. Our calculation includes the computation of all relevant
   transport coefficients in terms of the parameters of the models.
   Copyright (C) EPLA, 2014},
