@article{ WOS:000367632800004,
Author = {Antonyan, Sergey A. and Jonard-Perez, Natalia and Juarez-Ordonez, Saul},
Title = {Hyperspaces of convex bodies of constant width},
Journal = {TOPOLOGY AND ITS APPLICATIONS},
Year = {2015},
Volume = {196},
Number = {B},
Pages = {347-361},
Month = {DEC},
Note = {International Conference on Topology and Geometry held Jointly with the
   6th Japan-Mexico Topology Symposium, Matsue, JAPAN, SEP 02-06, 2013},
Organization = {Grant Aid Sci Res; Symposium Gen Topol},
Abstract = {For every n >= 1, let cc(R-n) denote the hyperspace of all non-empty
   compact convex subsets of the Euclidean space R-n endowed with the
   Hausdorff metric topology. For every non-empty convex subset D of {[}0,
   infinity) we denote by cw(D)(R-n) the subspace of cc(R-n) consisting of
   all compact convex sets of constant width d is an element of D and by
   crwD(R-n) the subspace of the product cc(R-n) x cc(R-n) consisting of
   all pairs of compact convex sets of constant relative width d is an
   element of D. In this paper we prove that cw(D)(R-n) and erw(D)(R-n) are
   homeomorphic to D X R-n x Q, whenever D not equal \{0\} and n >= 2,
   where Q denotes the Hilbert cube. In particular, the hyperspace cw(R-n)
   of all compact convex bodies of constant width as well as the hyperspace
   crw(R-n) of all pairs of compact convex sets of constant relative
   positive width are homeomorphic to Rn+1 X Q. (C) 2015 Elsevier B.V. All
   rights reserved.},
