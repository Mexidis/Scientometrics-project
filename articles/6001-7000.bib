Title = {AMS 14C DATING OF MATERIALS RECOVERED FROM THE TUNNEL UNDER
   THE TEMPLE OF THE FEATHERED SERPENT IN TEOTIHUACAN, MEXICO},
Journal = {RADIOCARBON},
Year = {2017},
