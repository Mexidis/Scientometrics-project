@article{ WOS:000362881900007,
Author = {Sanchez-Sanchez, L. and Escobar, M. L. and Sandoval-Ramirez, J. and
   Lopez-Munoz, H. and Fernandez-Herrera, M. A. and Hernandez-Vazquez, J.
   M. V. and Hilario-Martinez, C. and Zenteno, E.},
Title = {Apoptotic and autophagic cell death induced by glucolaxogenin in
   cervical cancer cells},
Journal = {APOPTOSIS},
Year = {2015},
Volume = {20},
Number = {12},
Pages = {1623-1635},
Month = {DEC},
Abstract = {The antiproliferative and cytotoxic activity of glucolaxogenin and its
   ability to induce apoptosis and autophagy in cervical cancer cells are
   reported. We ascertained that glucolaxogenin exerts an inhibitory effect
   on the proliferation of HeLa, CaSki and ViBo cells in a dose-dependent
   manner. Analysis of DNA distribution in the cell-cycle phase of tumor
   cells treated with glucolaxogenin suggests that the anti-proliferative
   activity of this steroid is not always dependent on the cell cycle.
   Cytotoxic activity was evaluated by detection of the lactate
   dehydrogenase enzyme in supernatants from tumor cell cultures treated
   with the steroid. Glucolaxogenin exhibited null cytotoxic activity. With
   respect to the apoptotic activity, the generation of apoptotic bodies,
   the presence of active caspase-3 and annexin-V, as well as the DNA
   fragmentation observed in all tumor lines after treatment with
   glucolaxogenin suggests that this compound does indeed induce cell death
   by apoptosis. Also, a significantly increased presence of the LC3-II,
   LC3 and Lamp-1 proteins was evidenced with the ultrastructural existence
   of autophagic vacuoles in cells treated with this steroidal glycoside,
   indicating that glucolaxogenin also induces autophagic cell death. It is
   important to note that this compound showed no cytotoxic effect and did
   not affect the proliferative capacity of mononuclear cells obtained from
   normal human peripheral blood activated by phytohaemagglutinin. Thus,
   glucolaxogenin is a compound with anti-proliferative properties that
   induces programmed cell death in cancer cell lines, though it is
   selective with respect to normal lymphocytic cells. These findings
   indicate that this glycoside could have a selective action on tumor
   cells and, therefore, be worthy of consideration as a therapeutic
   candidate with anti-tumor potential.},
