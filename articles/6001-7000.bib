@article{ WOS:000385146000019,
Author = {Munteanu, Marian Ioan and Palmas, Oscar and Ruiz-Hernandez, Gabriel},
Title = {Minimal Translation Hypersurfaces in Euclidean Space},
Journal = {MEDITERRANEAN JOURNAL OF MATHEMATICS},
Year = {2016},
Volume = {13},
Number = {5},
Pages = {2659-2676},
Month = {OCT},
Abstract = {The graph of a function f defined in some open set of the Euclidean
   space of dimension (p + q) is said to be a translation graph if f may be
   expressed as the sum of two independent functions I center dot and psi
   defined in open sets of the Euclidean spaces of dimension p and q,
   respectively. We obtain a useful expression for the mean curvature of
   the graph of f in terms of the Laplacian, the gradient of I center dot
   and psi as well as of the mean curvatures of their graphs. We study
   translation graphs having zero mean curvature, that is, minimal
   translation graphs, by imposing natural conditions on I center dot and
   psi, like harmonicity, minimality and eikonality (constant norm of the
   gradient), giving several examples as well as characterization results.},
