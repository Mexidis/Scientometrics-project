@article{ WOS:000358459700004,
Author = {Bayard, Pierre and Patty, Victor},
Title = {Spinor representation of Lorentzian surfaces in R2,,2},
Journal = {JOURNAL OF GEOMETRY AND PHYSICS},
Year = {2015},
Volume = {95},
Pages = {74-95},
Month = {SEP},
Abstract = {We prove that an isometric immersion of a simply connected Lorentzian
   surface in R-2,R-2 is equivalent to a normalised spinor field solution
   of a Dirac equation on the surface. Using the quaternions and the
   Lorentz numbers, we also obtain an explicit representation formula of
   the immersion in terms of the spinor field. We then apply the
   representation formula in R-2,R-2 to give a new spinor representation
   formula for Lorentzian surfaces in 3-dimensional Minkowski space.
   Finally, we apply the representation formula to the local description of
   the flat Lorentzian surfaces with flat normal bundle and regular Gauss
   map in R-2,R-2, and show that these surfaces locally depend on four real
   functions of one real variable, or on one holomorphic function together
   with two real functions of one real variable, depending on the sign of a
   natural invariant. (C) 2015 Elsevier B.V. All rights reserved.},
