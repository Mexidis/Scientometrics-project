@article{ WOS:000357595700009,
Author = {Hernandez-Meja, Claudia and Flores-Gallardo, Adrian and
   Llorente-Bousquets, Jorge},
Title = {Morphology of the Chorion in Leptophobia (Lepidoptera: Pieridae)
   and its Taxonomic Importance},
Journal = {SOUTHWESTERN ENTOMOLOGIST},
Year = {2015},
Volume = {40},
Number = {2},
Pages = {351-368},
Month = {JUN},
Abstract = {We describe and compare the chorion of seven species of the genus
   Leptophobia (Butler, 1870): L. eleusis eleusis (Lucas, 1852); L. tovaria
   maruga Fruhstorfer, 1907; L. cinerea cinerea (Hewitson, 1867); L. caesia
   caesia (Lucas, 1852); L. eleone luca Fruhstorfer, 1907; L. philoma
   subargentea (Hewitson, 1870); and L. aripa elodia (Boisduval, 1836). The
   description and illustration consider the following structural features:
   number and arrangement of axes (short and long), quantification of ribs,
   and distinction of bulges along the perimicropylar region. We also
   describe the color and size (length and its ratio to the maximum
   diameter) of the egg. Three shapes of eggs are typical: oblong
   ellipsoidal, barrel, and obovate. We offer comparisons between
   characters belonging to different species; taxonomic and phylogenetic
   implications are discussed, given that chorion characters, wing
   patterns, and microhabitats of the imagos are mutually correlated.
   Altogether this leads us to acknowledging two subgroups within the genus
   (Klots's hypothesis).},
