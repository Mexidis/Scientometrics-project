@article{ WOS:000369482100004,
Author = {Molina, Bertha and Tlahuice-Flores, Alfredo},
Title = {Thiolated Au18 cluster: preferred Ag sites for doping,
   structures, and optical and chiroptical properties},
Journal = {PHYSICAL CHEMISTRY CHEMICAL PHYSICS},
Year = {2016},
Volume = {18},
Number = {3},
Pages = {1397-1403},
Month = {JAN 21},
Abstract = {Recently, the X-ray determined structure of the thiolated Au-18 cluster
   has been reported. In this communication, we addressed a study of
