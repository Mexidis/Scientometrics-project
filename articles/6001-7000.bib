@article{ WOS:000374197500001,
Author = {Lopez-Sandoval, Oscar and Montejano, Gustavo and Carmona, Javier and
   Cantoral, Enrique and Becerra-Absalon, Itzel},
Title = {Algal diversity from extreme environment: the geothermal spring Los
   Hervideros, Mexico},
Journal = {REVISTA MEXICANA DE BIODIVERSIDAD},
Year = {2016},
Volume = {87},
Number = {1},
Pages = {1-9},
Month = {MAR},
Abstract = {In this work we describe the algal diversity, which is in Los Hervideros
   geothermal spring, in Michoacan, Mexico, and its spatial distribution
   related with the thermal gradient. This spring forms a part of Los
   Azufres system, which is affected by volcanic activity from the
   Transversal Neovolcanic Belt. The species identification was
   morphological and physical and chemical parameters were measured as
   temperature, specific conductivity and pH. The temperature recorded was
   between 22.2 to 85.2 degrees C. The specific conductivity is as high as
   3,400 to 4,430 mu S cm(-1), while the pH is 7.5 to 8.0. The growth were
   dominated by the next Cyanophyceae species: Arthrospira sp.,
   Synechococcus sp. and Synechocystis sp. in temperatures from 22 to 76
   degrees C, respectively. As far as the Bacillariophyceae, we registered:
   Achnanthidium exiguum, Halamphora veneta, Anomoeoneis costata, Diploneis
   elliptica and Pinnularia viridis in temperatures from 22 to 55 degrees
   C. We also registered one Chlorophyceae species, Oedogonium sp. (30 to
   35 degrees C). This spring is characterized by the presence of great
   changes of temperate in a few centimeters, which is reflected in a
   heterogeneous distribution of the algal species. All Rights Reserved (C)
   2016 Universidad Nacional Autonoma de Mexico, Institute de Biologia.
   This is an open access item distributed under the Creative Commons CC
