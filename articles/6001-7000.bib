@article{ WOS:000350518100008,
Author = {Bernal-Santos, D. and Tamariz-Mascarua, A.},
Title = {The Menger property on Cp(X, 2)},
Journal = {TOPOLOGY AND ITS APPLICATIONS},
Year = {2015},
Volume = {183},
Pages = {110-126},
Month = {MAR 15},
Abstract = {A space X is said to have the Menger property (or simply X is Menger) if
   for every sequence < u(n) : n is an element of omega > of open covers of
   X, there exists a sequence of finite sets < F-n : n is an element of
   omega > such that boolean OR(n is an element of omega) F-n is a cover of
   X and F-n subset of u(n), for every n is an element of w. We prove: (1)
   If X is a subspace of C-p(Y), where Y-n is Menger for every n is an
   element of omega, and X' (the set of non-isolated points of X) is
   compact, then C-p(X, 2) is Menger; (2) If Cp(X, 2) is Menger and X is
   normal, then X' is countably compact; (3) For a first countable GO-space
   without isolated points L, C-p(L, 2) is Menger if and only if C-p(L, 2)
   is Lindelof and L is countably compact; and for a subspace L of an
   ordinal, C-p(L, 2) is Menger if and only if C-p(L, 2) is Lindelof and L'
   is countably compact; (4) For every F is an element of omega{*},
   C-p(omega boolean OR\{F\},2) is Menger if and only if F is a strong
   P-point; (5) Assuming the Continuum Hypothesis, there is a maximal
   almost disjoint family A for which the space C-p(Psi(A), 2) is Menger.
   (C) 2014 Elsevier B.V. All rights reserved.},
