@article{ WOS:000373412000052,
Author = {Cruz, J. and Andrade, E. and Muhl, S. and Canto, C. and de Lucio, O. and
   Chavez, E. and Rocha, M. F. and Garces-Medina, E.},
Title = {Ion beam analysis and co-sputtering simulation (CO-SS) of bi-metal films
   produced by magnetron co-sputtering},
Journal = {NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION B-BEAM
   INTERACTIONS WITH MATERIALS AND ATOMS},
Year = {2016},
Volume = {371},
Pages = {268-272},
