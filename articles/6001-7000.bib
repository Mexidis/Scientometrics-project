   Montejano, Amanda, Univ Nacl Autonoma Mexico, Fac Ciencias, UMDI, Mexico City, DF, Mexico.},
DOI = {10.1016/j.dam.2015.03.008},
ISSN = {0166-218X},
EISSN = {1872-6771},
Keywords = {Perfection in 3-hypergraphs; Transitivity in 3-hypergraphs;
   Comparability 3-hypergraphs},
Research-Areas = {Mathematics},
Web-of-Science-Categories  = {Mathematics, Applied},
