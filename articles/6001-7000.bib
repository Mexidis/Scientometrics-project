@article{ WOS:000368278400006,
Author = {Cristians, Sol and Reyna Osuna-Fernandez, H. and Ramirez-Avila,
   Guillermo and Munoz-Ocotero, Veronica and Laguna-Hernandez, Guillermo
   and Brechu-Franco, Alicia E.},
Title = {Euphorbia dioeca Kunth as a novel source for α-glucosidase
   inhibitors},
Journal = {BOLETIN LATINOAMERICANO Y DEL CARIBE DE PLANTAS MEDICINALES Y AROMATICAS},
Year = {2015},
Volume = {14},
Number = {6},
Pages = {483-490},
Month = {NOV},
Abstract = {Euphorbia dioeca Kunth belongs to the Wanderer's herb complex that is
   traditionally used for skin diseases and recently as antidiabetic. The
   methanol and aqueous extracts were evaluated for their in vitro
   gamma-glucosidase inhibitory activity and an oral starch tolerance test.
   These extracts showed an IC50 of 0.55 and 0.85 mg/mL, respectively. In
   diabetic Long Evans rats, the methanol and aqueous extracts reduced
   significantly the postprandial hyperglycemia peak in 15.2\% and 12.8\%,
   respectively. The gamma-glucosidase inhibitory activity is related with
   the presence of glycosides, phenolic compounds and flavonoids.
   Additionally, the safety parameters of both extracts were assessed by
   means of an acute toxicity test, being classified as innocuous. The
   traditional use of E. dioeca to control type 2 diabetes was confirmed,
   being an important source of gamma-glucosidase inhibitors.},
