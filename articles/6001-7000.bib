@article{ WOS:000379227800018,
Author = {Nieves-Uribe, Sandra and Flores-Gallardo, Adrian and Llorente-Bousquets,
   Jorge},
Title = {Chorionic Morphology in Eleven Species of Biblidinae from Mexico and
   Colombia: a Test of Predictions},
Journal = {SOUTHWESTERN ENTOMOLOGIST},
Year = {2016},
Volume = {41},
Number = {2},
Pages = {505-532},
Month = {JUN},
Abstract = {The description and relatively detailed illustration of the chorion of
   the Biblidinae subfamily is complemented, with the addition of 11
   different species from those recently presented of the genera Hamadryas,
   Marpesia, Eunica, Catonephele, Myscelia, Asterope, Epiphile, Callicore,
   and Cyclogramma; they were collected from Mexico and Colombia. The study
   was from a sample of 35 females of which 15 had mature eggs. The major
   structural features of the chorion were characterized: color, shape,
   size, and relief: perimicropilar differentiation and formation of axes
   and ribs. A distinction was made over specialized traits in the apical
   and equatorial zones as well as the degree of thickening of the axes.
   Types of eggs recently recognized are followed with the nomenclature of
   the descriptions. The results confirm architecture specific and features
   to each suprageneric taxon. It supported the proposal to use the chorion
   as a taxonomic character, because each genus and species has unique
   characteristics. Also supported, one more time, was the possibility of
   resurgance of the subtribe Eunicina of Reuter, by separating Eunica and
   other genera of the Epicaliina subtribe. The chorionic similitude of
   Cyclogramma and Diaethria sustain the proposal of Lamas to consider both
   as synonyms.},
