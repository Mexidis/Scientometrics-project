@article{ WOS:000401014100016,
Author = {Polanco, Carlos and Buhse, Thomas and Vizcaino, Gloria and Levy
   Picciotto, Jacobo},
Title = {The polar profile of ancient proteins: a computational extrapolation
   from prebiotics to paleobiochemistry},
Journal = {ACTA BIOCHIMICA POLONICA},
Year = {2017},
Volume = {64},
Number = {1},
Pages = {117-122},
Abstract = {This paper addresses the polar profile of ancient proteins using a
   comparative study of amino acids found in 25 000 000-year-old shells
   described in Abelson's work. We simulated the polar profile with a
   computer platform that represented an evolutionary computational toy
   model that mimicked the generation of small proteins starting from a
   pool of monomeric amino acids and that included several dynamic
   properties, such as self-replication and fragmentation-recombination of
   the proteins. The simulations were taken up to 15 generations and
   produced a considerable number of proteins of 25 amino acids in length.
   The computational model included the amino acids found in the ancient
   shells, the thermal degradation factor, and the relative abundance of
   the amino acids observed in the Miller-Urey experimental simulation of
   the prebiotic amino acid formation. We found that the amino acid polar
   profiles of the ancient shells and those simulated and extrapolated from
   the Miller-Urey abundances are coincident.},
