@article{ WOS:A1996UF97600004,
Author = {Medina, A and Trevino, C},
Title = {Continuum equations for the granular flow on a free surface},
Journal = {REVISTA MEXICANA DE FISICA},
Year = {1996},
Volume = {42},
Number = {2},
Pages = {193-208},
Month = {APR},
Abstract = {In this paper we present a discussion on the continuum approach to the
   equations of motion for the gravity induced flow in a free surface of a
   noncohesive granular material. We emphasize the usefulness of the
   continuum approach to describe the fully dynamic or grain-inertia and
   the quasi-static regimes. In order to justify the validity of the
   proposed equations, we introduce a model for the dissipative stresses
   occurring in the flow. We also discuss some of the analytical solutions
   of these equations.},
