@article{ WOS:A1983QP10500009,
Author = {PARDO, A and ROSENSTEIN, I and MONTFORT, I and PEREZTAMAYO, R},
Title = {IMMUNOHISTOCHEMICAL IDENTIFICATION OF COLLAGENASE IN CARRAGEENIN
   GRANULOMA},
Journal = {JOURNAL OF HISTOCHEMISTRY \& CYTOCHEMISTRY},
Year = {1983},
Volume = {31},
Number = {5},
Pages = {641-646},
Abstract = {The collagenase present in experimental carrageenin granuloma in the
   guinea pig has been purified to homogeneity in acrylamide gel
   electrophoresis by a combination of ammonium sulfate salting out and
   affinity chromatography on Sepharose 4B-collagen-packed columns. The
   single protein band thus obtained was used as an antigen to obtain a
   monospecific antibody in heterologous conditions. Several
   immunodiffusion, immunoaffinity chromatography and immunoinhibition
   tests of the antibody against the  specific antigen and various possible
   serum and tissue contaminants suggested that the antibody was
   specifically directed against the enzyme protein collagenase. Indirect
   immunohistochemical staining of carrageenin granulomas, sampled at
   different developmental phases with this specific anti-collagenase
   antibody, revealed that the specific antigenic protein (the enzyme
   collagenase) is universally present on the extracellular structures at
   both the collagen-deposition and the collagen-resorption  stages.
   Apparently, the enzyme collagenase is bound to its substrate (collagen)
   under both normal and pathological conditions, and the critical point of
   control of collagen degradation must be the activation of the
   collagen-bound enzyme.},
