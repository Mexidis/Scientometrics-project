@article{ WOS:001607242000001,
Author = {Pulido Silva, Maria Teresa and Flores-Vazquez, Juan Carlos and
   Vargas-Zenteno, Maricela and Vite Reyes, Aurelia and Vovides, Andrew P.},
Title = {Spatial distribution pattern of the endangered endemic cycad Ceratozamia
   fuscoviridis (Zamiaceae) in central Mexico},
Journal = {PLANT SPECIES BIOLOGY},
Year = {2025},
Month = {2025 NOV 3},
Abstract = {Spatial distribution pattern was studied for Ceratozamia fuscoviridis,
   an endangered species in Mexico. The conservation importance of this
   species lies in the fact that it is endemic to a relatively small
   geographic area in which changes in land use negatively impact the
   populations. Based on Ripley's K (t) and K (12) analysis, the spatial
   distribution patterns of five populations of C. fuscoviridis were
   studied as follows: (a) the population as a whole (K (t) function); (b)
   by each size class (K (t) function); and (c) between pairs of categories
   (K (12) function). For C. fuscoviridis, the distribution pattern of the
   group of individuals was aggregated. Cluster patterns are related to a
   lack of dispersal and/or indicate places more suitable for cycads.
   Comparison between categories (Ripley's K (12)) indicated that they were
   mainly independent but sometimes dependent and, at certain distances,
   there was inhibition. It is the first record of inhibition in cycads in
   Mexico, suggesting intraspecific competition. The value of Ripley's K
   (t) and K (12) for conservation purpose is discussed. The results of the
   spatial distribution analysis can be applied to restoration strategies
   to facilitate conservation of the species.},
