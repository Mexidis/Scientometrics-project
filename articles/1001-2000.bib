@article{ WOS:A1991GY21700047,
Author = {MARQUINA, V and MARQUINA, ML and JIMENEZ, M and ABURTO, S and GOMEZ, R
   and ESCUDERO, R},
Title = {SYSTEMATIC MOSSBAUER STUDY OF YBA2CU3-XFEXO-DELTA AND PRBA2CU3-XFEXO
   DELTA AS A FUNCTION OF TEMPERATURE},
Journal = {HYPERFINE INTERACTIONS},
Year = {1991},
Volume = {66},
Number = {1-4},
Pages = {423-428},
Note = {LATIN AMERICAN CONF ON THE APPLICATIONS OF THE MOSSBAUER EFFECT ( LACAME
   90 ), HAVANA, CUBA, OCT 29-NOV 02, 1990},
Organization = {UN IND DEV ORG; INT CTR THEORET PHYS; LATIN AMER CTR PHYS; UNESCO;
   NISHIN STEEL; WISSENSCH ELEKTR; AMERSHAM INT},
Abstract = {We study the similitudes and differences between YBa2Cu3-xFexO-delta and
   PrBa2Cu3-xFexO-delta systems, using Mossbauer spectroscopy. Both
   systems, with z = 0.04, were studied at several temperatures. The
   spectra of YBa2Cu3-xFeO-delta showed four asymmetric quadrupole doublets
   with apparently different line widths, which were fitted with four
   symmetric superimposed quadrupole doublets with the same line width, but
   assuming a small (approximately 4-6 kOe) residual magnetic field in the
   Fe sites, which are mainly the Cu(1) sites of the 1-2-3 structure. On
   the other hand, the PrBa2Cu3-xFeO-delta spectra showed only three
   asymmetric quadrupole doublets which can be fitted with three
   superimposed symmetric doublets and a residual magnetic field. In this
   case, the Fe atoms also occupy the Cu(l) sites of the 1-2-3 structure.
   The temperature variations of the Mossbauer spectra and their parameters
   seem to indicate that, in the Pr-system, a structural change takes place
   between 12 K and 40 K.},
