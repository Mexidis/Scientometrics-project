@article{ WOS:A1996VF23100004,
Author = {Pardo, A and Selman, M},
Title = {Matrix metalloproteinases and lung injury},
Journal = {BRAZILIAN JOURNAL OF MEDICAL AND BIOLOGICAL RESEARCH},
