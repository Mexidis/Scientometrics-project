@article{ WOS:A1994NY74600007,
Author = {GARCIAFERREIRA, S and TAMARIZMASCARUA, A},
Title = {P-FRECHET-URYSOHN PROPERTY OF FUNCTION-SPACES},
Journal = {TOPOLOGY AND ITS APPLICATIONS},
Year = {1994},
Volume = {58},
Number = {2},
Pages = {157-172},
Month = {JUL 22},
Abstract = {In this paper, we study the p-Frechet-Urysohn property of function
   spaces, for p is-an-element-of beta(omega)\textbackslash{}omega. We
   prove that C(pi)(X) is p-Frechet-Urysohn if and only if X has
   (gamma(p)), where (gamma(p) is the natural p-version of property (gamma)
   (this is a generalization of a result due to Gerlits and Nagy). We note
   the following implications: X is second countable double arrow pointing
   right X has (gamma(p)) for some p is-an-element-of
   beta(omega)\textbackslash{}omega double line arrow pointing right X(n)
   is Lindelof for all 1 less-than-or-equal-to n < omega. We deal with the
   question when is C(pi)(R) a p-Frechet-Urysohn space. It is shown that
   there is p is-an-element-of beta(omega)\textbackslash{}omega such that
   C(pi)(R) is p-Frechet-Urysohn; if p is semiselective, then every subset
   X of R satisfying (gamma(p)) has measure zero and if p is selective,
   then X is a strong measure zero set; and we can find p is-an-element-of
   beta(omega)\textbackslash{}omega such that C(pi)(R) is p-Frechet-Urysohn
   and is not strongly p-Frechet-Urysohn. Finally, we prove that R(omega)
   does not have (gamma(p)) whenever p is a P-point of
   beta(omega)\textbackslash{}omega.},
