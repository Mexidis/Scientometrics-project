@article{ WOS:001524903400001,
Author = {Martinez-Cadena, J. A. and Tamariz-Mascarua, A.},
Title = {Some properties involving feeble compactness, IV : Compactness-like
   properties defined via dense subspaces},
Journal = {TOPOLOGY AND ITS APPLICATIONS},
Year = {2025},
Volume = {373},
Month = {NOV 1},
Abstract = {We study several compactness-like properties arising from the existence
   of dense subspaces satisfying relative compactness conditions, namely:
   countably pracompact, totally countably pracompact, densely
   omega-bounded, and sequentially pracompact spaces. These classes refine
   classical notions such as sequential compactness and omega-boundedness
   and admit a natural hierarchy. We establish various preservation results
   for these properties under perfect open mappings and product spaces. In
   the context of topological groups, we prove that if His a locally
   compact subgroup of a topological group G, then the corresponding
   quotient mapping pi : G -> G/H allows the transfer of local versions of
   these properties from G/H to G. We also analyze the extent to which
   these properties satisfy the three-space property and introduce the
   class of PC-spaces to characterize when such transfer is possible.
   Finally, we address structural questions on densely omega-bounded
   paratopological groups and provide conditions under which they are
   topological. (c) 2025 Elsevier B.V. All rights are reserved, including
   those for text and data mining, AI training, and similar technologies.},
