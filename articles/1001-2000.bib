@article{ WOS:A1980KW02500005,
Author = {GRANADOS, H},
Title = {VITAMIN-E AND METHYLENE-BLUE IN THE PREVENTION OF ABNORMAL PIGMENTATION
   IN THE SEMINAL-VESICLES OF THE CASTRATED GOLDEN-HAMSTER},
Journal = {ARCHIVOS DE INVESTIGACION MEDICA},
Year = {1980},
Volume = {11},
Number = {3},
Pages = {361-375},
Abstract = {The possibility of preventing the abnormal, yellow-brown pigment that is
   present in the seminal vesicles of the castrated hamster was possible by
   means of vitamin E (20-500 mg\%), methylene blue (126 mg\%), SeO2
   (20-200 mcg\%) and licidril (dimethylaminoethyl-p-chlorophenol and
   acetate, 1\%) given in the basic diet was examined. Prepubertal hamsters
   (strain Ch CM) were used; they were fed during 120 experimental days
   Purine laboratory chow as a basic diet and were castrated at the
   beginning of the experiment. Apparently, 100-500 mg\% of vitamin E
   levels and methylene blue at the administered level prevents the
   macroscopic evidence of this pigment whereas SeO2 and licidril do not
   prevent the formation of this pigment at the levels administrated.
   Different aspects and implications of the results that establish the
   existence of important interrelations between physiology and pathology
   of the male hamster reproductive system and metabolism of vitamin E and
   methylene blue, another non-biological antioxidant, are discussed.},
