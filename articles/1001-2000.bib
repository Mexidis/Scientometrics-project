@article{ WOS:001609606300001,
Author = {Lopez-Reyes, Kevin and Kass, Jamie M. and Yanez-Arenas, Carlos and
   Villalobos, Fabricio},
Title = {Historical biogeography and climatic niche dynamics in spiny lizards of
   the genus Sceloporus (Squamata: Phrynosomatidae)},
Journal = {BIOLOGICAL JOURNAL OF THE LINNEAN SOCIETY},
Year = {2025},
Volume = {146},
Number = {3},
Month = {NOV},
Abstract = {Although we now have extensive biogeographical datasets for tetrapods,
   unravelling the drivers of species' distributions remains challenging,
   especially for many squamate groups. Spiny lizards of the genus
   Sceloporus exemplify this knowledge gap as their geographical origin has
   been the topic of long-standing debates. In addition, we lack
   information about the historical dispersal and the evolutionary dynamics
   of the responses to climate of this genus. To help fill these gaps, we
   used an approach that combined phylogenetic regionalization, ancestral
   range estimation, age-range correlation, and trait evolution analyses.
   Our results indicate that Sceloporus likely originated from a wide
   region spanning tropical and temperate climates of North and Central
   America, followed by speciation in sympatry at the bioregional level.
   However, patterns of geographical overlap among clades suggest the
   potential role of both sympatric and allopatric processes. We also found
   indications that interregional dispersal events are decoupled from
   evolutionary shifts in niche breadth and position, suggesting
   long-distance dispersal under niche conservatism. Together, these
   findings point to local extinctions and interregional dispersal as key
   forces structuring current Sceloporus distributions at the bioregional
   level and more broadly underscore the importance of integrating
   ecological, evolutionary, and biogeographical factors to better
   understand what shapes species' distributions.},
