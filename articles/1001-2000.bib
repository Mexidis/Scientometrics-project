@article{ WOS:000835538400001,
Author = {Rios-Herrejon, Alejandro},
Title = {Singular precalibers for topological products},
Journal = {TOPOLOGY AND ITS APPLICATIONS},
Year = {2022},
Volume = {317},
Month = {AUG 1},
Abstract = {In this paper we prove that if kappa is a singular cardinal with
   uncountable cofinality, then every power of a given topological space
   with precaliber kappa has precaliber kappa as well. Furthermore, if
   \{X-alpha : alpha < lambda\} is a family of topological spaces with
   precaliber kappa and lambda < cf(kappa), then kappa is a precaliber for
   the topological product Pi\{X-alpha : alpha < lambda\}. (c) 2022
   Elsevier B.V. All rights reserved.},
