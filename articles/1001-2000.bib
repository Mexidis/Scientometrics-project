@article{ WOS:001550480700008,
Author = {Cordoba-Cuevas, Enya Astrid and Chan, Pak Tsun and Renton, Katherine},
Title = {A Reptile Feast: Diet of the Collared Forest-Falcon in a Tropical Dry
   Forest of Western Mexico},
Journal = {JOURNAL OF RAPTOR RESEARCH},
Year = {2025},
Volume = {59},
Number = {3},
Month = {SEP},
Abstract = {.-Knowledge of raptor diets is crucial for understanding predator-prey
   dynamics and avian ecology. However, for the widely distributed Collared
   Forest-Falcon (Micrastur semitorquatus), little is known of the species'
   diet in the northern part of its distributional range. We determined the
   diet of the Collared Forest-Falcon in the tropical dry forest of western
   Mexico by direct observations of prey deliveries during 80 hr of video
   recordings at eight nests over two breeding seasons (2021-2022) and by
   identifying prey bone remains in an additional nest cavity in a fallen
   tree. We observed 28 prey deliveries from videos and field observations
   and identified 298 prey bone remains. Both methods highlighted the
   predominance of reptiles (82.1\%, n = 23 observations; 57.1\%, n = 170
   bone remains), particularly large-bodied iguanas, in the diet of nesting
   Collared Forest-Falcons in tropical dry forest. Birds composed 38.9\% (n
   = 116) of prey bone remains and 10.7\% (n = 3) of observations. The West
   Mexican Chachalaca (Ortalis poliocephala), the largest cracid in the
   region, was the most common avian prey, although direct observations
   detected significantly fewer avian prey than found in prey remains.
   Mammals made up little of the prey (7\%, n = 2 observations; 4\%, n = 12
   bone remains) and consisted mostly of Virginia opossum (Didelphis
   virginiana), but included at least two individuals of the endangered
   Magdalena woodrat (Xenomys nelsoni), a micro-endemic species. Our
   results differ from previous studies that found a predominance of
   mammalian and avian prey in Collared Forest-Falcon diets and elucidate
   dietary variations of the species throughout its distribution.},
