@article{ WOS:000730159400001,
Author = {Garcia-Rueda, Adriana and Tremblay, Nelly and Mascaro, Maite and Diaz,
   Fernando and Paschke, Kurt and Caamal-Monsreal, Claudia and Rosas,
   Carlos},
Title = {The thermal tolerance of a tropical population of blue crab
   (Callinectes sapidus) modulates aerobic metabolism during hypoxia},
Journal = {JOURNAL OF THERMAL BIOLOGY},
Year = {2021},
Volume = {102},
Month = {DEC},
Abstract = {The blue crab Callinectes sapidus is a widespread ectothermic species
   that supports large fisheries. Physiology of temperate and subtropical
   populations of blue crabs are well studied; however, a lack of
   information exists on tropical populations. Given the low locomotion
   capabilities of C. sapidus adult blue crabs, natural selection should
   favor traits that shape a particular thermal niche reflected through
   tolerance modulation to dissolved oxygen (DO). This study was designed
   to evaluate the thermal window and hypoxia sensitivity of the blue crab
   population in the southern Gulf of Mexico. The effect of acclimation
   temperatures from 20 degrees C to 34 degrees C on thermal preference
   (TP), critical thermal limits (CT), and thermal metabolic scope (TMS)
   was assessed in normoxia. Metabolic rate regulation over oxygen partial
   pressure (pO(2)) gradient was evaluated through oxygen consumption
   measurements at different degrees of acute hypoxia. Callinectes sapidus
   was observed tending to specialize towards higher temperatures, showing
   a mean TP from 26 degrees C to 33 degrees C. The lowest performance of
   aerobic pathways was observed at the coldest regimes and the highest at
   the warmest ones with mean TMS value being 35 \% greater at 34 degrees C
   than 20 degrees C. Patterns for metabolic regulation were dependent on
   the interaction between environmental temperature and DO, in which the
   interval from 29 degrees C to 34 degrees C provoked a 50 \% reduction in
   oxygen consumption when exposed to similar to 20\% air saturation
   levels. The results obtained showed that blue crabs distributed in the
   southern Gulf of Mexico could be close to their oxygen-temperature
   tolerance limits, which has important implications when climate change
   effects on species re-distribution is considered.},
