@article{ WOS:000874773700016,
Author = {Angel Lopez-Mucino, Luis and Garcia-Garcia, Fabio and Cueto-Escobedo,
   Jonathan and Acosta-Hernandez, Mario and Venebra-Munoz, Arturo and
   Carlos Rodriguez-Alba, Juan},
Title = {Sleep loss and addiction},
Journal = {NEUROSCIENCE AND BIOBEHAVIORAL REVIEWS},
Year = {2022},
Volume = {141},
Month = {OCT},
Abstract = {Reducing sleep hours is a risk factor for developing cardiovascular,
   metabolic, and psychiatric disorders. Furthermore, previous studies have
   shown that reduction in sleep time is a factor that favors relapse in
   addicted patients. Additionally, animal models have demonstrated that
   both sleep restriction and sleep deprivation in-crease the preference
   for alcohol, methylphenidate, and the self-administration of cocaine.
   Therefore, the present review discusses current knowledge about the
   influence of sleep hours reduction on addictivebehaviors; likewise, we
   discuss the neuronal basis underlying the sleep reduction-addiction
   relationship, like the role of the orexin and dopaminergic system and
   neuronal plasticity (i.e., delta FosB expression). Potentially, chronic
   sleep re-striction could increase brain vulnerability and promote
   addictive behavior.},
