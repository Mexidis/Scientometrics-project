@article{ WOS:000517656500003,
Author = {Fuentes Guillen, Elias and Martenez Adame, Carmen},
Title = {The notion of variable quantities ω in Bolzano's early works},
Journal = {HISTORIA MATHEMATICA},
Year = {2020},
Volume = {50},
Pages = {25-49},
Month = {FEB},
Abstract = {Amid the debate over infinitesimals, Bolzano introduced the alternative
   notion of variable quantities omega in his 1816 work on the binomial
   theorem. It has often been assumed that his 1817 definition of
   continuity using them is practically the modern one. This paper explores
   Bolzano's early mathematical works and diaries to gain insight into the
   subtleties in his definition of w and some of his mathematical
   procedures. We show that those quantities are not clearly
   `proto-Weierstrassian' and argue that Bolzano was in the process of
   refinement and increasing abstraction of the idea of quantity that
   eventually led to the development of a theory of real numbers. (C) 2019
   Elsevier Inc. All rights reserved.},
