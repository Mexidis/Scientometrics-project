@article{ WOS:A1995RA89800004,
Author = {TOME, B},
Title = {ITERATED COIL ENLARGEMENTS OF ALGEBRAS},
Journal = {FUNDAMENTA MATHEMATICAE},
Year = {1995},
Volume = {146},
Number = {3},
Pages = {251-266},
Abstract = {Let Lambda be a finite-dimensional, basic and connected algebra over an
   algebraically closed field, and mod Lambda be the category of finitely
   generated right Lambda-modules. We say that Lambda has acceptable
   projectives if the indecomposable projective Lambda-modules lie either
   in a preprojective component without injective modules or in a standard
