@article{ WOS:001553838200012,
Author = {Ivonne Flores, B. S. and Glebskiy, Yury and Cano-Santana, Zenon},
Title = {ARRIVAL AND DISPERSION OF THE INVASIVE PLANT PERSICARIA NEPALENSIS IN
   MEXICO; IT'S TOO LATE TO STOP},
Journal = {SOUTHWESTERN NATURALIST},
Year = {2025},
Volume = {69},
Number = {3},
Month = {SEP},
Abstract = {Persicarla nepalensis is an important invasive plant that is spreading
   in many locations across the globe. In Mexico, it was first reported in
   2005; however, the species has not been studied since the first arrival
   report. Therefore, the aim of this article is to report new observations
   of the plant and analyze the citizen science reports to evaluate the
   current status of this species in Mexico. We used data from the GBIF
   database and analyzed the number of reports historically, during the
   year, and the current geographical distribution. We also collected a
   specimen in a new location. We found that the number of observations is
   increasing over time and that the distribution area of this plant is
   already at least 79,681 km2. Most observations were made in the autumn,
   suggesting that this is the main flowering time. We conclude that the
   invasive plant is spreading across Mexico and that the window of
   opportunity for eradication of the species is closed. Therefore,
   mitigation plans should be prepared.},
