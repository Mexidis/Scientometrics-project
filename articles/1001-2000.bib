@article{ WOS:001388101700016,
Author = {Colin-Garcia, Norberto A. and Ocana-Mendoza, Camila and Chiappa-Carrara,
   Xavier and Rioja-Nieto, Rodolfo and Arena Ortiz, M. Leticia and
   Calle-Trivino, Johanna and Alvarado-Recillas, Natalia and E Campos,
   Jorge},
Title = {Thermal stress response in the Montastraea cavernosa
   coral-symbiont complex in the Mexican Caribbean, through the expression
   of the HSP70 gene},
Journal = {BULLETIN OF MARINE SCIENCE},
Year = {2024},
Volume = {100},
Number = {4},
Pages = {779-791},
Month = {OCT},
Abstract = {Coral reefs are under constant and increasing environmental stress.
   Symbiotic relationships between coral and dinoflagellates of the family
   Symbiodiniaceae help corals resist stressors and survive. Therefore, it
   is essential to implement conservation and restoration programs that use
   genetic diversity and response mechanisms at the molecular level of
   corals and their symbionts. This study assessed the thermal stress
   response through the expression of the heat shock protein 70 (HSP70) in
   Montastraea cavernosa corals and their symbionts from the Mexican
   Caribbean. We observed variation in the expression level and that corals
   have high HSP70 levels, while symbionts exhibit low HSP70 values.
   Additionally, the ability of the symbiont to acclimatize to thermal
   stress conditions is evident, which may allow the coral to resist
   environmental stressors; a joint response in both host and symbionts can
   also reduce cell damage and prevent bleaching. Detecting and spreading
   stress-resistant organisms can enhance coral reef restoration by
   reducing bleaching, mortality, and promoting adaptive genes.},
