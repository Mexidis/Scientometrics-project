@article{ WOS:000589892700007,
Author = {Dorantes-Aldama, A. and Shakhmatov, D.},
Title = {Reprint of: Compactness properties defined by open-point games},
Journal = {TOPOLOGY AND ITS APPLICATIONS},
Year = {2020},
Volume = {281},
Number = {SI},
Month = {AUG 1},
Abstract = {Let S be a topological property of sequences (such as, for example, ``to
   contain a convergent subsequence{''} or ``to have an accumulation
   point{''}). We introduce the following open-point game OP(X,S) on a
   topological space X. In the nth move, Player O chooses a non-empty open
   set U-n subset of X, and Player P responds by selecting a point x(n) is
   an element of U-n. Player P wins the game if the sequence \{x(n) : n is
   an element of N\} satisfies property S in X; otherwise, Player O wins.
   The (non-)existence of regular or stationary winning strategies in OP(X,
   S) for both players defines new compactness properties of the underlying
   space X. We thoroughly investigate these properties and construct
   examples distinguishing half of them, for an arbitrary property S
   sandwiched between sequential compactness and countable compactness. (C)
   2020 Elsevier B.V. All rights reserved.},
