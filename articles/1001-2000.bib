@article{ WOS:A1984TZ33500028,
Author = {PEREZ, TM and ATYEO, WT},
Title = {FEATHER MITES, FEATHER LICE, AND THANATOCHRESIS},
Journal = {JOURNAL OF PARASITOLOGY},
Year = {1984},
Volume = {70},
Number = {5},
Pages = {807-812},
Abstract = {Unrelated taxa of feather mites use exuviae of their own, or closely
   related, species for molting within quills or on feather surfaces of
   diverse groups of birds. On the Orange-fronted Conure {[}Aratinga
   canicularis] and other parrot species, feather mites and mallophagan
   eggs coinhabit the same feather area, mites molt within the exuviae of
   other mites within the mallophagan egg shells. Molting within exuviae
   and within egg shells are examples of thanatochresis (the use of dead
   parts of one animal by living individuals of a 2nd species, but not for
   food). This phenomenon may be common among feather mites and other
   Astigmata.},
