@article{ WOS:A1986F274900007,
Author = {FIGUERAS, T and PARDO, A},
Title = {COLLAGEN BIOSYNTHESIS AND DEGRADATION DURING DEPOSIT AND RESORPTIVE
   PHASES OF CARRAGEENAN GRANULOMA},
Journal = {COLLAGEN AND RELATED RESEARCH},
Year = {1986},
Volume = {6},
Number = {4},
Pages = {379-385},
Abstract = {Collagen metabolism was studied in both, the developing and resorptive
   phases of carrageenin granuloma. The rate of collagen biosynthesis
   relative to the rate of total protein synthesis is almost twice as high
   during the deposit phase compared with the resorptive phase of the same
   process. Total collagenolytic activity appears to be approximately the
   same in both, deposit and resorption stages of the granuloma.
   Notwithstanding, the enzyme is fully active during resorption; whereas
   half of this activity is in a latent form during deposit. Collagenolytic
   activity increased remarkably when assayed at 30 mM CaCl2 as compared to
   10 or 50 mM.},
