   Sanchez-Bringas, Federico, Univ Nacl Autonoma Mexico, Fac Ciencias, Dept Matemat, Ciudad Univ, Mexico City 04510, DF, Mexico.},
DOI = {10.1007/s13398-015-0254-2},
