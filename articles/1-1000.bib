@article{ WOS:000258751900003,
Author = {Pellicer-Covarrubias, Patricia},
Title = {1/2-homogeneity in symmetric products},
Journal = {TOPOLOGY AND ITS APPLICATIONS},
Year = {2008},
Volume = {155},
Number = {15},
Pages = {1650-1660},
Month = {SEP 1},
Abstract = {A space is 1/m-homogeneous provided there are exactly m orbits for the
   action of the group of homeomorphisms of the space onto itself. In this
   paper we investigate 1/2-homogeneity of the 2-fold symmetric product
   F-2(X) for a continuum X. We prove that if X is a 1-dimensional,
   compact, connected ANR, then F-2(X) is 1/2-homogeneous if and only if X
   is an arc or a simple closed curve. We also show that this
   characterization does not generalize to higher dimensions. Further, we
   give necessary and sufficient conditions under which the 2-fold
   symmetric product of an n-manifold is 1/2-homogeneous. (C) 2008 Elsevier
   B.V. All rights reserved.},
