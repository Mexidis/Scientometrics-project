@article{ WOS:000418013400029,
Author = {Gerardo Avila-Canto, Jose and Velazquez-Mendoza, Carolina and
   Castelblanco-Martinez, Nataly and Nino-Torres, Carlos and Cordova-Tapia,
   Fernando},
Title = {Is the Antillean manatee (Trichechus manatus manatus) back in
   town? Presence of the species at the ``Area de Proteccion de Flora y
   Fauna YumBalam{''}, Quintana Roo, Mexico},
Journal = {REVISTA MEXICANA DE BIODIVERSIDAD},
Year = {2017},
Volume = {88},
Number = {4},
Pages = {999-1002},
Month = {DEC},
Abstract = {The Antillean manatee (Trichechus manatus manatus), considered
   endangered, is protected at national (Mexico) and international levels.
   Since Hurricane Gilbert in 1988, sightings of manatees have been rare at
   Conil Lagoon, located within the natural reserve ``Area de Proteccion de
   Flora y Fauna Yum Balam{''}. However, increased reports of sightings
   from local fishermen and tour guides suggest manatees may be
   repopulating the. lagoon Here we report 15 sightings of the Antillean
   manatee from 2011 to 2016. Group sizes ranged from 1 to 15 individuals
   and we documented the presence of reproductive groups and mother-calf
   pairs. The apparent recovery of the species at Conil Lagoon and adjacent
   areas may be the resulit of local conservation efforts, or the result of
   changes in habitat use and distribution of the Caribbean population. A
   systematic, long-term local monitoring program is necessary to estimate
   manatee abundance and characterize their distribution in this area. (C)
   2017 Universidad Nacional Autonoma de Mexico, Instituto de Biologia.},
Publisher = {INST BIOLOGIA, UNIV NACIONAL AUTONOMA MEXICO},
Address = {APARTADO POSTAL 70-233, MEXICO, D F 00000, MEXICO},
Type = {Article},
Language = {English},
Affiliation = {Córdova-Tapia, F (Corresponding Author), Univ Nacl Autonoma Mexico, Inst Biol, Apartado Postal 70-153, Mexico City 04510, DF, Mexico.
   Gerardo Avila-Canto, Jose, Manaholchi SC, Paseo Kuka Mz 171,L-2, Isla Holbox 77310, Quintana Roo, Mexico.
