@article{ WOS:000295913300021,
Author = {Hernandez-Perez, Ezequiel and Gonzalez-Espinosa, Mario and Trejo, Irma
   and Bonfil, Consuelo},
Title = {Distribution of the genus Bursera in Morelos state (Mexico) and
   its relation to climate},
Journal = {REVISTA MEXICANA DE BIODIVERSIDAD},
Year = {2011},
Volume = {82},
Number = {3},
Pages = {964-976},
Month = {SEP},
Abstract = {The influence of climatic variables on the distribution of 15 species of
   Bursera in Morelos Mexico, was analyzed. Data on the presence of species
   and climatic variables from 47 meteorological stations were obtained,
   and maps of actual and potential distribution were made using a GIS; the
   influence of the main climatic variables on the distribution was
   analyzed by principal component and cluster analyses. The species of
   Bursera are found in an altitudinal range of 800-2 800 m; species
   richness is higher in the warm, relatively dry areas south of the state.
   Two main groups of species were distinguished: a), those having a
   restricted distribution, found mainly in warm sites and, b), those
   having a wider distribution, including relatively cool sites.
   Precipitation, maximum and minimum temperatures, evapotranspiration and
   its seasonality, all influenced the distribution of the species. Bursera
   bicolor, B. bipinnata, B. cuneata, B. copallifera, B. fagaroides, B.
   grandifolia and B. glabrifolia are found on sites having a relatively
   low minimum temperature (5-10 degrees C) and high precipitation, while
   B. aptera, B. ariensis, B. lancifolia, B. linanoe, B. longipes, B.
   tnorelensis, B. schlechtendalii and B. submoniliformis are found in
   areas experiencing higher minimum temperatures (>= 14 degrees C) and
   relatively drier conditions.},
