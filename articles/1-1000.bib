@article{ WOS:001536957200001,
Author = {Wences, Giovanni and Salazar-Flores, Yuri and Hernandez-Lopez, Eymard},
Title = {Warehouse Location and Distribution Planning Through Ramified Optimal
   Transport},
Journal = {IEEE TRANSACTIONS ON INTELLIGENT TRANSPORTATION SYSTEMS},
Year = {2025},
Volume = {26},
Number = {11},
Pages = {20421-20434},
Month = {NOV},
Abstract = {This article introduces a novel approach to addressing supply chain and
   logistics challenges using ramified optimal transport. This framework
   provides a flexible and efficient means of modeling nonlinear and
   realistic scenarios within natural and complex transportation networks,
   particularly in real-time applications where route optimization is
   critical. The problem considered involves determining optimal warehouse
   locations and devising distribution strategies for delivering goods from
   suppliers to customers. To this end, we developed two new estimation
   algorithms and benchmarked them against traditional and heuristic
   methods. The proposed algorithms demonstrated competitive performance in
   solving supply chain and logistics problems, with one of them achieving
   the best results in terms of computational cost. Finally, the algorithms
   were applied to real-world data from the cement industry, successfully
   producing real-time optimal routes. This was made possible by the
   incorporation of a carefully designed distance function in the cost
   modeling.},
