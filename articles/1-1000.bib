@article{ WOS:000399165800008,
Author = {Miguel Valenzuela, Luis and Falconi, Manuel and Ble, Gamaliel},
Title = {A Generalist Predator and the Planar Zero-Hopf Bifurcation},
Journal = {INTERNATIONAL JOURNAL OF BIFURCATION AND CHAOS},
Year = {2017},
Volume = {27},
Number = {3},
Month = {MAR},
Abstract = {A typical approach for searching periodic orbits of planar dynamical
   systems is through the Hopf bifurcation. In this work we present a
