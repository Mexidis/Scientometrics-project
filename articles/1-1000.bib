@article{ WOS:001478346400001,
Author = {Bayard, Pierre and Seppi, Andrea},
Title = {Entire hypersurfaces of constant scalar curvature in Minkowski space},
Journal = {JOURNAL FUR DIE REINE UND ANGEWANDTE MATHEMATIK},
Year = {2025},
Volume = {2025},
Number = {824},
Pages = {167-201},
Month = {JUL 1},
Abstract = {We show that every regular domain D in Minkowski space R n , 1 which is
   not a wedge admits an entire hypersurface whose domain of dependence is
   D and whose scalar curvature is a prescribed constant (or function,
   under suitable hypotheses) in ( - infinity , 0 ). Under rather general
   assumptions, these hypersurfaces are unique and provide foliations of D.
   As an application, we show that every maximal globally hyperbolic Cauchy
   compact flat spacetime admits a foliation by hypersurfaces of constant
   scalar curvature, generalizing to any dimension previous results of
   Barbot-B \& eacute;guin-Zeghib (for n = 2 n=2 ) and Smith (for n = 3 n=3
   ).},
