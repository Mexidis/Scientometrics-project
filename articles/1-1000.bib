@article{ WOS:000814060500001,
Author = {Martinez-Serrano, Raymundo G. and Nunez-Velazquez, Miriam V. and
   Contreras-Cruz, Diana and Garcia-Tovar, Gloria P. and Torres-Peralta,
   Melissa A. and Solis-Pichardo, Gabriela and Canet, Carles},
Title = {On the unusual presence of a Quaternary peralkaline volcanic center,
   rear-arc region of the Trans-Mexican Volcanic Belt eastern sector:
   geochemical and isotopic characterization of the Las Navajas-Hidalgo
   stratovolcano},
Journal = {INTERNATIONAL JOURNAL OF EARTH SCIENCES},
Year = {2022},
Volume = {111},
Number = {6},
Pages = {1983-2015},
Month = {SEP},
Abstract = {The Quaternary Las Navajas-Hidalgo stratovolcano was constructed,
   between similar to 2.58 and similar to 2.2 Ma, by peralkaline rhyolites
   (comendites), with scarce quartz, sanidine, and plagioclase phenocrysts,
   emplaced similar to 120 km behind the volcanic front of the
   Trans-Mexican Volcanic Belt eastern sector. First, the peralkaline
   Pre-Collapse Event built a broad volcano (similar to 2.58 Ma ago) upon
   Miocene calc-alkaline dacites of the Sierra de Pachuca. Then, after a
   lateral-collapse explosion, the peralkaline Post-Collapse Event (similar
   to 2.2 Ma ago) produced pyroclastic deposits and glassy rhyolites.
   Besides, 22 mafic scoria cones with scarce olivine, clinopyroxene, and
   plagioclase phenocrysts, grouped in the Monogenetic Volcanic Event
   (similar to 2.58 and similar to 0.2 Ma), settled almost simultaneously
   with the stratovolcano. The trace-element patterns for the peralkaline
   rhyolites display enrichment of REE, HFSE, and Zr-Hf, but with highly
   negative Ba, Sr, P, Ti, and Eu anomalies that are not typical for
   subduction-related rocks. While mafic scoria cone samples lack negative
   HFSE anomalies relative to LILE, similar to OIB, except for their
   positive Sr, K, and Pb anomalies. Initial Sr and Nd isotopic values
   display a large range for peralkaline rhyolites suggesting some crustal
   interaction (Sr-87/Sr-86 = 0.70514-0.73093 and Nd-143/Nd-144 =
   0.51267-0.51271). While the Pb isotopes (Pb-206/Pb-204 = 18.80-18.92;
   Pb-207/Pb-204 = 15.61-15.72 and (208)pb/Pb-204 38.63-38.99) indicate a
   Pb-207-rich reservoir (Paleozoic basement rocks). On the other hand, the
   Sr and Nd isotopic values for the mafic Monogenetic Volcanic Event rocks
   fall into the mantle array (Sr-87/Sr-86 = 0.70384-0.70534 and
   Nd-143/Nd-144 = 0.51270-0.51283), whereas the Pb isotopic (Pb-206/Pb-204
   = 18.72-18.84; Pb-207/Pb-204 = 15.59-15.62 and Pb-208/Pb-204 =
   38.45-38.65) show a mixing line between a mantle-like component and
   radiogenic continental crust. The evidence suggests that the peralkaline
   rhyolites were formed through prolonged fractional crystallization
   processes in shallow reservoirs (similar to 5 kbars), and under
   water-undersaturated conditions (similar to 0.2\% water), from basaltic
   melts as those formed the Monogenetic Volcanic Event. These melts are
   probably related to the subduction slab rollback process that triggered
   asthenospheric components' upwelling towards the lithospheric mantle
   wedge and produced vulcanism in the rear-arc region.},
