@article{ WOS:000912287000001,
Author = {Vinagre, M. R. and Ble, G. and Esteva, L.},
Title = {Dynamical Analysis of a Model for Secondary Infection of the Dengue},
Journal = {DIFFERENTIAL EQUATIONS AND DYNAMICAL SYSTEMS},
Year = {2025},
Volume = {33},
Number = {1},
Pages = {43-69},
Month = {JAN},
Abstract = {Dengue disease is a major public health problem in the world with a fast
   spreading rate. Human migration has contibute to spread of the different
   serotypes of dengue virus, incrementing the risk of dengue hemorrhagic
   fever and dengue shock syndrome. The disease immunological response is
   complex, with severe disease linked to the antibody -dependent
   enhancement. In this work we propose a system of differential equations
   to model the impact of human migration on the spread of two serotypes of
   dengue virus between two regions where only one of the serotypes was
   initially present. When the individuals to the other region they can
   contract a different serotype. Using the next generation matrix method,
   the basic reproductive number R-0 is calculated and disease-free
   equilibrium stability is determined. In addition, conditions are
   obtained that guarantee the existence of an endemic equilibrium. These
   results provide conditions to predict an epidemic outbreak of dengue
   hemorrhagic due to a secondary infection of dengue.},
