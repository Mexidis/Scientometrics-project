@article{ WOS:000335829800018,
Author = {Alberto Rincon-Mejia, Hugo and Shaid Sandoval-Miranda, Martha Lizbeth},
Title = {On pseudocomplements and supplements in the big lattice of preradicals},
Journal = {JOURNAL OF ALGEBRA AND ITS APPLICATIONS},
Year = {2014},
Volume = {13},
Number = {7},
Month = {NOV},
Abstract = {In this paper, we consider aspects of the big lattice of preradicals,
   related to pseudocomplements and supplements. We consider essential
   preradicals and superfluous preradicals, and we characterize the
   situation in which all nonzero preradicals are essential as well as the
   one in which all proper preradicals are superfluous. Also, we give two
