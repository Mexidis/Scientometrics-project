@article{ WOS:001397110500001,
Author = {Biro, Gabor and Serkin, Leonid and Paic, Guy and Barnafoldi, Gergely
   Gabor},
Title = {Investigating the soft and hard limits in transverse momentum spectra in
   pp collisions},
