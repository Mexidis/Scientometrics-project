@article{ WOS:001263536100001,
Author = {Guzman-Pro, Santiago and Hernandez-Cruz, Cesar},
Title = {Orientations without forbidden patterns on three vertices},
Journal = {APPLIED MATHEMATICS AND COMPUTATION},
Year = {2024},
Volume = {480},
Month = {NOV 1},
Abstract = {Given a set F of oriented graphs, a graph G is a Forb e (F) -graph if it
   admits an F -free orientation. Skrien showed that proper -circular arc
   graphs, nested interval graphs and comparability graphs, correspond to
   Forb e (F) -graph classes for some set F of orientations of P 3 .
   Building on these results, we exhibit the list of all Forb e (F) -graph
   classes when F is a set of oriented graphs on three vertices. Structural
   characterizations for these classes are provided, except for the
   so-called perfectly-orientable graphs and the
   transitive-perfectly-orientable graphs, which remain as open problems.},
