@article{ WOS:A1989U331300006,
Author = {MARQUEZGUZMAN, J and ENGLEMAN, M and MARTINEZMENA, A and MARTINEZ, E and
   RAMOS, C},
Title = {REPRODUCTIVE ANATOMY OF LACANDONIA-SCHISMATICA (LACANDONIACEAE)},
Journal = {ANNALS OF THE MISSOURI BOTANICAL GARDEN},
Year = {1989},
Volume = {76},
Number = {1},
Pages = {124-127},
Abstract = {Lacandonia schismatica comprises a new family, genus, and species. Its
   most conspicuous character is the central position of the androecium,
   surrounded by the apocarpous gynoecium. This arrangement has not been
   recorded before for angiosperms. The androceium consists of three
   (sometimes four) bilocular anthers with introrse dehiscence. The
   gynoecium is apocarpic. Each carpel contains a single ovule that is
   sessile, basal, anatropous and bitegmic; the inner integument forms the
   micropyle. The position of the style on the ovary changes during
   development. The endosperm is of the nuclear type. The embryo of the
   mature seed is poorly developed. The fruit is indehiscent.},
