@article{ WOS:000334090000005,
Author = {Rojas-Hernandez, R.},
Title = {On monotone stability},
Journal = {TOPOLOGY AND ITS APPLICATIONS},
Year = {2014},
Volume = {165},
Pages = {50-57},
Month = {MAR 15},
Abstract = {The notion of monotonically monolithic space was introduced by V.V.
   Tkachuk in 2009 {[}8]. In this paper we introduce the notion of monotone
   stability and show that a space C-p (X) is monotonically monolithic if
   and only if X is monotonically stable. As a consequence, a space C-p (X)
   is monotonically stable if and only if X is monotonically monolithic. We
   also prove that C-p (X) is monotonically monolithic when X is a
   Sigma(k)-product of a family of Lindelof Sigma-spaces. These results
   answer some questions posed by A. Tamariz-Mascarua and the author in a
   previous paper {[}7]. (C) 2014 Elsevier B.V. All rights reserved.},
