@article{ WOS:001502405100001,
Author = {Serrano-Juarez, D. L. and Miranda, J. and Pineda, J. C. and
   Reynoso-Cruces, S.},
Title = {Silver L X-ray production cross sections induced by 12C and
   13C ions},
Journal = {RADIATION PHYSICS AND CHEMISTRY},
Year = {2025},
Volume = {236},
Month = {NOV},
Abstract = {L X-ray production cross sections of Ag induced by the collision of
   12C3+ and 13C3+ ions with energies between 2.6 MeV and 4.0 MeV were
   measured using thin targets. The different masses of the ions allowed
   the observation of dissimilarities in the cross sections which can be
   explained by Coulomb deflection and binding energy effects. Moreover,
   when the cross sections are plotted as a function of a reduced variable
   v1/v2L, where v1 is the ion velocity, and v2L is the average (Bohr)
   electron orbital velocity in the L shell, they closely follow a single
   curve, even as compared to previously published experimental data.
   Several theoretical models were also applied to compare with the results
   from this experiment, considering corrections caused by electron capture
   and multiple ionization, as well as the united atom effect. Two distinct
   atomic parameters databases were also employed. It was impossible to
   determine the theoretical model that describes the data with better
   accuracy, both the X-ray production cross sections and their ratios,
   because of the uncertainties; no significant difference was noted with
   the atomic parameters databases.},
