@article{ WOS:000664394600002,
Author = {CRUZ DURaN, R. A. M. I. R. O. and ANDRADE MURGUiA, G. L. O. R. I. A.},
Title = {Lonchocarpus chavelasii (Leguminosae, Papilionoideae,
   Millettieae), a new species from the state of Guerrero, Mexico},
Journal = {BRITTONIA},
Year = {2020},
Volume = {73},
Number = {3},
Pages = {311-315},
Month = {SEP},
Abstract = {Lonchocarpus chavelasii, a new species from the state of Guerrero,
   Mexico, is described and illustrated. Lonchocarpus chavelasii is similar
   to L. eriophyllus, however it is distinguished from the latter by
   presenting leaves with a smaller number of leaflets (3-5 vs. 13-21),
   leaflets with craspedodromous venation (vs. camptodromous), corolla with
   pellucid dots (vs. without pellucid dots), longer floral peduncles (8-18
   mm vs. 3-5 mm), longer bracteoles (3-4 mm, linear vs. 1 mm, oblong),
   fruits thickened on the adaxial side and oblong (vs. flattened and
   linear), and sub-spherical seeds (vs. flattened).},
