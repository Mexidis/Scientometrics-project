@article{ WOS:000919709700001,
Author = {Duran, Ramiro Cruz and Murguia, Gloria Andrade},
Title = {Lonchocarpus foliolosus (Leguminosae, Papilionoideae,
   Millettieae), a new species from Guerrero, Mexico},
Journal = {BRITTONIA},
Year = {2023},
Volume = {75},
Number = {1},
Pages = {56-61},
Month = {MAR},
Abstract = {Lonchocarpus foliolosus, a new species from the state of Guerrero,
   Mexico, is described and illustrated. Lonchocarpus foliolosus is similar
   to L. canoensis, but it differs from that species by presenting
   sub-obovate to elliptic leaflets, with a smaller number of secondary
   veins per side ((5-)6-8(-10)), paniculate inflorescences, the standard
   with white sericeous pubescence, narrower legumes with an obtuse apex
   and with a greater number of seeds (up to five), and in its distribution
   restricted to the state of Guerrero, Mexico.},
