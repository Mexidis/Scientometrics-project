@article{ WOS:000227665700002,
Author = {Cota, LG and de la Mora, P},
Title = {On the structure of lithium peroxide, Li2O2},
Journal = {ACTA CRYSTALLOGRAPHICA SECTION B-STRUCTURAL SCIENCE CRYSTAL ENGINEERING
   AND MATERIALS},
Year = {2005},
Volume = {61},
Number = {2},
Pages = {133-136},
Month = {APR},
Abstract = {The two published lithium peroxide structures, both ascribed to the
   hexagonal P (6) over bar space group, were subjected to
   reinterpretation, and another more symmetric structure, now belonging to
   the P6(3)/mmc space group, was found. Detailed density-functional
   quantum mechanical calculations and crystal structure optimizations were
