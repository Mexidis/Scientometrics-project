@article{ WOS:000233380300005,
Author = {Ludlow-Wiechers, B and Almeida-Leñero, L and Islebe, G},
Title = {Paleoecological and climatic changes of the Upper Lerma Basin, Central
   Mexico during the Holocene},
Journal = {QUATERNARY RESEARCH},
Year = {2005},
Volume = {64},
Number = {3},
Pages = {318-332},
