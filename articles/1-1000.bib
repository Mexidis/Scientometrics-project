@article{ WOS:A1987K764700001,
Author = {MAINERO, A and CARDENAS, R and VALDES, V and GRANADOS, H},
Title = {STUDIES ON THE BIOLOGY OF THE VOLCANO MOUSE
   (NEOSTOMODON-ALSTONI-ALSTONI) .12. COMPARATIVE HEMATOLOGICAL PARAMETERS
   OF WILD AND LABORATORY-BORN (F1) ANIMALS},
Journal = {ARCHIVOS DE INVESTIGACION MEDICA},
Year = {1987},
Volume = {18},
Number = {3},
Pages = {181-186},
Abstract = {An initial study of the hematological values, serum iron and
   2,3-diphosphoglycerate content of wild and laboratory-born (F1) volcano
   mice (Neotomodon alstoni alstoni) is reported. The hematological values
   showed no noticeable differences between wild and F1, mice, nor among
   females and males. However, some small but statistically significative
   differences were found between wild and F1 males in the hematocrit, mean
   corpuscular hemoglobin concentration (MCHC) and in the leukocyte count. 
   The F1 differential leukocyte count showed significantly higher
   percentages of lymphocytes and monocytes, with a concurrent reduction of
   the neutrophil percentage, both in males and in females, compared to the
   wild mice. The comparison of the hematological values of Neotomodon with
   the ones reported for other laboratory rodents, indicates that the
   volcano mouse has a lower erythrocyte corpuscular volume, and hence a
   lower hematocrit value. The hemoglobin, mean corpuscular hemoglobin,
   mean corpuscular hemoglobin concentration and erythrocyte count of
   Neotomodon resemble the values published for other rodents. Serum iron
   values are similar in wild and F1 mice, both in males and in females, in
   spite of the different diet. 2,3-diphosphoglycerate content was
   significatively increased in the wild females with respect to males;
   this gender differences does not prevail in the laboratory population,
   which showed levels similar to those exhibited by the wild male mice.},
