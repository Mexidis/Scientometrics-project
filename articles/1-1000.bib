@article{ WOS:000561707800003,
Author = {Boronski, Jan P. and Illanes, Alejandro and Marquez, Emanuel R.},
Title = {Connected neighborhoods in Cartesian products of solenoids},
Journal = {FUNDAMENTA MATHEMATICAE},
Year = {2020},
Volume = {248},
Number = {3},
Pages = {309-320},
Abstract = {Given a collection of pairwise co-prime integers m(1), ..., m(r) > 1, we
   con- sider the product Sigma = Sigma(m1) x ... x Sigma(mr) , where each
   Sigma(mi) is the m(i)-adic solenoid. Answering a question of D. P.
   Bellamy and J. M. Lysko, we prove that if M is a subcontinuum of Sigma
   such that the projections of M on each Sigma(mi) are onto, then for each
   open subset U in Sigma with M subset of U, there exists an open
   connected subset V of Sigma such that M subset of V subset of U, i.e.
   any such M is ample in the sense of Prajs and Whittington (2007). This
