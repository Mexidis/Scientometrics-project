@article{ WOS:001233902300001,
Author = {Miranda-Rangel, A. and Cano-Santana, Z. and Castano-Meneses, G. and
   Palacios-Vargas, J. G. and Corona-Tejeda, P.},
Title = {EDAPHIC COLLEMBOLA (COLLEMBOLA) AS INDICATORS OF THE CHEMICAL
   CHARACTERISTICS OF SOIL IN A PEACH ORCHARD (PRUNUS PERSICA
   (L.) BATSCH) IN MICHOACAN, MEXICO},
Journal = {APPLIED ECOLOGY AND ENVIRONMENTAL RESEARCH},
Year = {2024},
Volume = {22},
Number = {3},
Pages = {2601-2625},
Abstract = {Edaphic collembola are abundant in agricultural soils and can be used as
   bioindicators of the condition of the soils they inhabit. We study the
   composition, diversity, and phenology of the collembola community in a
   peach orchard ( Prunus persica (L.) Bach) located in Zit \&
   aacute;cuaro, Michoac \& aacute;n, Mexico. We evaluated the effect of
   physical and chemical characteristics of the orchard's soil on the
   edaphic collembola community. Collembola were extracted by Berlese
   funnels from samples of leaf litter and soil obtained monthly, from
   September 1993 to August 1994. Fallen leaves were not removed from the
   ground for the duration of the study. Sixty-nine species were
   registered: 58 in leaf litter and 53 in soil. Collembola density varied
   from 248 to 15,889 ind m(-2) in leaf litter and 419 to 7,286 ind m(-2)
   in soil. The layer of fallen leaves exhibited the highest density,
   species richness and diversity of collembola. The edaphic variables that
   altered collembola density were organic matter content, cationic
   exchange capacity (CEC) and carbon-tonitrogen ratio. Richness was
   affected by organic matter and total nitrogen. Diversity was affected by
   pH, total nitrogen, calcium, and carbon-to-nitrogen ratio. The density
   of dominant species was negatively affected by phosphorus and calcium
   content, as well as CEC.},
