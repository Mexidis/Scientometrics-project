@article{ WOS:001434394200001,
Author = {Medina-Barcenas, Mauricio and Rincon-Mejia, Hugo},
Title = {m-Endoregular lattices},
Journal = {COMMUNICATIONS IN ALGEBRA},
Year = {2025},
Volume = {53},
Number = {2},
Pages = {554-581},
Month = {FEB 1},
Abstract = {In a previous work, (dual)-m-Rickart lattices were studied. Now, in this
   paper, we introducem-endoregular lattices as those lattices L such that
   m is a regular monoid, where m is a submonoid with zero of End(lin)(L).
   We show that these lattices can be characterized in terms of m-Rickart
   and dual-m-Rickart lattices. Also, we compare these new lattices with
   those lattices in which every compact element is a complement. We
   characterize the m-endoregular lattices such that every idempotent in m
   is central in m and we show that for these lattices the complements are
   a sublattice which is a Boolean algebra. We introduce two new concepts,
   m-K-extending and m-T-lifting lattices. For these lattices, we show that
   the monoid m has a regular quotient monoid provided they satisfy m-C-2
   and m-D-2 respectively. At the end, some applications to quotients
   categories of R-Mod are given.},
