@article{ WOS:000385938100002,
Author = {Camargo, H. A. and Socolovsky, M.},
Title = {Rindler approximation to Kerr-Newman black hole},
Journal = {EUROPEAN PHYSICAL JOURNAL PLUS},
Year = {2015},
Volume = {130},
Number = {11},
Month = {NOV 19},
Abstract = {We show that the Rindler approximation to the time-radial part of the
   Kerr and Kerr-Newman metrics near their external h(+) and internal h(-)
   horizons only holds outside h(+) and inside h(-), so respectively inside
   and outside the external and internal ergospheres, regions where, in
   Boyer-Lindquist coordinates, both g(tt) and g(rr) are negative, but
   preserving the Lorentzian character of the metric, and r > 0 i.e.
   outside the region r < 0 where closed timelike curves exist. At each
   point, the choice of Rindler coordinates is not trivial, but depends on
   the polar angle theta. The approximation, as is known, automatically
   gives the absolute values of the surface gravities kappa(+/-) as the
   corresponding proper accelerations, and therefore the Hawking
   temperatures tau(+/-) at h(+/-).},
