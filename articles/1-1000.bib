@article{ WOS:001565084300001,
Author = {Martinez-Montejano, Jorge M. and Mendez, Hector and Velazquez-Inzunza,
   Yajaida N.},
Title = {The hyperspace ω(f) when f is a transitive dendrite
   mapping},
Journal = {EUROPEAN JOURNAL OF MATHEMATICS},
Year = {2025},
Volume = {11},
Number = {3},
Month = {AUG 28},
Abstract = {Let X be a compact metric space. By 2(X) we denote the hyperspace of all
   closed and non-empty subsets of X endowed with the Hausdorff metric. Let
   f: X -> X be a continuous function. We study some topological properties
   of the hyperspace omega(f), the collection of all omega limits sets w
   (x, f) with x is an element of X. We prove the following: (i) If X has
   no isolated points, then, for every continuous function f: X -> X,
   int(2)(X) (omega(f)) = empty set. (ii) If X is a dendrite for which
   every arc contains a free arc and f: X -> X is transitive, then the
   hyperspace omega(f) is totally disconnected. (iii) Let D-infinity be the
   Wazewski's universal dendrite. Then there exists a transitive continuous
   function f: D-infinity -> D-infinity for which the hyperspace omega( f )
   contains an arc; hence, omega( f ) is not totally disconnected.},
