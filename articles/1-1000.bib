@article{ WOS:000257200700002,
Author = {Hrusak, M. and Sanchis, M. and Tamariz-Mascarua, A.},
Title = {Ultrafilters and non-Cantor minimal sets in linearly ordered dynamical
   systems},
Journal = {ARCHIVE FOR MATHEMATICAL LOGIC},
Year = {2008},
Volume = {47},
Number = {3},
Pages = {193-203},
Month = {JUL},
Abstract = {It is well known that infinite minimal sets for continuous functions on
   the interval are Cantor sets; that is, compact zero dimensional
   metrizable sets without isolated points. On the other hand, it was
   proved in Alcaraz and Sanchis (Bifurcat Chaos 13:1665-1671, 2003) that
   infinite minimal sets for continuous functions on connected linearly
   ordered spaces enjoy the same properties as Cantor sets except that they
   can fail to be metrizable. However, no examples of such subsets have
   been known. In this note we construct, in ZFC, 2(c) non-metrizable
   infinite pairwise nonhomeomorphic minimal sets on compact connected
   linearly ordered spaces.},
