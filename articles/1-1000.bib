@article{ WOS:000446710900012,
Author = {Sour-Tovar, Francisco and Alicia Quiroz-Barroso, Sara and Angel
   Torres-Martinez, Miguel},
Title = {Occurrence of Plagiobrissus grandis (Gmelin, 1788) (Echinoidea:
   Spatangoida) in the Tuxpan Formation, Miocene (Langhian) in nortern
   Veracruz state, Mexico. Biogeographic considerations},
Journal = {BOLETIN DE LA SOCIEDAD GEOLOGICA MEXICANA},
Year = {2018},
Volume = {70},
Number = {3},
Pages = {797-807},
Abstract = {The presence of Plagiobrissus grandis is reported in a locality of the
   middle Miocene (Langhiano) of the Tuxpan Formation in the
   Papantla-Martinez de la Torre region, North of Veraacruz. The finding
   allows to analyze the space-time distribution of the genus. and
   corroborates its origin in the Caribbean during Eocene times, as well as
   its later dispersion during the Miocene to North Carolina in the United
   States and the coasts of Venezuela and Brazil. The divi sion of the
   Pacific and the Atlantic oceans during the Miocene-Pliocene, bp the
   emergence of the Panama Isthmus, triggered great changes in the marine
   currents of the North Atlantic; that event favored the dispersion of the
   genus from the Miocene to the Recent towards the Mediterranean and the
   coasts of North Africa, to the Guff of Guinea. The finding of
   Plagiobrissus grandis in the middle Miocene and its survival until the
   present provides an example of the longevity of several echinoid
   species.},
