@article{ WOS:000697040000001,
Author = {Recillas-Roman, Stephanie and Montano, Martha and Ruiz, Victor and
   Perez-Ramos, Julia and Becerril, Carina and Herrera, Iiana and
   Amador-Munoz, Omar and Margarita Martinez-Dominguez, Y. and Ramos,
   Carlos},
Title = {Wood Smoke Extract Promotes Extracellular Matrix Remodeling in Normal
   Human Lung Fibroblasts},
Journal = {INTERNATIONAL JOURNAL OF TOXICOLOGY},
Year = {2021},
Volume = {40},
Number = {6},
Pages = {506-516},
Month = {DEC},
Abstract = {Wood smoke (WS) contains many harmful compounds, including polycyclic
   aromatic hydrocarbons (PAHs). WS induces inflammation in the airways and
   lungs and can lead to the development of various acute and chronic
   respiratory diseases. Pulmonary fibroblasts are the main cells involved
   in the remodeling of the extracellular matrix (ECM) during the
   WS-induced inflammatory response. Although fibroblasts remain in a low
   proliferation state under physiological conditions, they actively
   participate in ECM remodeling during the inflammatory response in
   pathophysiological states. Consequently, we used normal human lung
   fibroblasts (NHLFs) to assess the potential effects of the
   PAHs-containing wood smoke extract (WSE) on the growth rate, total
   collagen synthesis, and the expression levels of collagen I and III,
   matrix metalloproteinase (MMP)-1, MMP-2, MMP-9, tissue inhibitor of
   metalloproteinase (TIMP)-1, TIMP-2, and the transforming growth factor
   (TGF)-beta 1. We also assessed MMPs activity. The results showed that
   WSE induced a trimodal behavior in the growth rate curves in NHLFs; the
   growth rate increased with 0.5-1 \% WSE and decreased with 2.5\% WSE,
   without causing cell damage; 5-20\% WSE inhibited the growth and induced
   cell damage. After 3 hours of exposure, 2.5\% WSE induced an increase in
   total collagen synthesis and upregulation of TGF-beta 1, collagen I and
   III, MMP-1, TIMP-1, and TIMP-2 expression. However, MMP-2 expression was
   downregulated and MMP-9 was not expressed. The gelatinase activity of
   MMP-2 was also increased. These results suggest that WSE affects the ECM
   remodeling in NHLFs and indicate the potential involvement of PAHs in
   this process.},
