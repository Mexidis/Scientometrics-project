@article{ WOS:000236247200001,
Author = {Bueno-Hernández, AA and Llorente-Bousquets, JE},
Title = {The other face of Lyell: historical biogeography in his Principles of
   geology},
Journal = {JOURNAL OF BIOGEOGRAPHY},
Year = {2006},
Volume = {33},
Number = {4},
Pages = {549-559},
Month = {APR},
Abstract = {Although some excellent articles about Lyell's work have been published,
   they do not explicitly deal with Lyell's biogeographical conceptions.
   The purpose of this paper is to analyse Lyell's biogeographical model in
   terms of its own internal structure. Lyell tried to explain the
   distribution of organisms by appealing to a real cause (climate).
   However, he was aware that environmental conditions were clearly
   insufficient to explain the existence of biogeographical regions.
   Lyell's adherence to ecological determinism generated strong tensions
   within his biogeographical model. He shifted from granting a secondary
   weight to dispersal to assigning it a major role. By doing so, Lyell was
   led into an evident contradiction. A permanent tension in Lyell's ideas
   was generated by the prevalent explanatory pattern of his time. The
   explanatory model based on laws did not produce satisfactory results in
   biology because it did not deal with historical processes. We may
   conclude that the knowledge of organic distribution interested Lyell as
   long as it could be explained by the uniformitarian principles of his
   geological system. The importance of the second volume of the Principles
   of geology lies in its ample and systematic argumentation about the
   geographical distribution of organisms. Lyell established, independently
   from any theory about organic change, the first version of dispersalist
   biogeography.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
Type = {Editorial Material},
Language = {English},
Affiliation = {Bueno-Hernández, AA (Corresponding Author), Univ Nacl Autonoma Mexico, Fac Estudios Super Zaragoza, Museo Zool, Batalla 5 de Mayo S-N,Col Ejercito Oriente, Del Iztapalapa 09230, Mexico.
   Univ Nacl Autonoma Mexico, Fac Estudios Super Zaragoza, Museo Zool, Del Iztapalapa 09230, Mexico.
   Univ Nacl Autonoma Mexico, Fac Ciencias, Museo Zool Alfonso L Herrera, Mexico City 04510, DF, Mexico.},
DOI = {10.1111/j.1365-2699.2006.01475.x},
