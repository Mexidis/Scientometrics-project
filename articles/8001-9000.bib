@article{ WOS:000277096000020,
Author = {Domingues, P. and Garcia, S. and Hachero-Cruzado, I. and Lopez, N. and
   Rosas, C.},
Title = {The use of alternative prey (crayfish, Procambarus clarki, and hake,
   Merlucius gayi) to culture Octopus vulgaris (Cuvier 1797)},
Journal = {AQUACULTURE INTERNATIONAL},
Year = {2010},
Volume = {18},
Number = {3},
Pages = {487-499},
Month = {APR},
Abstract = {The effects of two alternative prey (crayfish and hake) were tested on
   growth and survival of both juveniles and adults of Octopus vulgaris in
   two experiments. Octopuses fed the control (squid) were larger (3.0 +/-
   A 0.7 kg) than those fed crayfish (2.4 +/- A 0.6 kg) at the end of
   experiment I. Similarly, overall growth rates were higher for octopuses
   fed squid (1.7 +/- A 0.3 and 1.2 +/- A 0.2 \%BW day(-1), respectively).
   Average feeding rates for the experiment were not different, being 6.5
   +/- A 0.9 and 7.5 +/- A 0.9 \%BW day(-1), respectively, for octopuses
   fed either squid or crayfish. Nevertheless, food conversions for the
   experiment were higher (42.4 +/- A 2.7\%) for octopuses fed squid
   compared to the ones fed crayfish (23.9 +/- A 1.9 g). For experiment II,
   hake and crayfish were compared to squid; the final weight of octopuses
   fed squid, hake or crayfish was 1,183.0 +/- A 242.7 g, 1,175.6 +/- A
   240.1 g and 922.3 +/- A 160.1 g, respectively. Overall growth rates for
   the experiment were 1.9 +/- A 0.2 \%BW day(-1), 1.9 +/- A 0.3 \%BW
   day(-1) and 1.1 +/- A 0.3 g, respectively. Final weight and growth rates
   were never different (P > 0.05) between octopuses fed squid and hake,
   but were always higher (P < 0.05) compared to the ones fed crayfish.
   Average feeding rates for experiment II were similar for the three
   diets, and of 4.6 +/- A 1.5, 4.2 +/- A 1.3 \%BW day(-1) and 5.1 +/- A
   0.9 \%BW day(-1), respectively, for octopuses fed squid, hake or
   crayfish. Food conversions for experiment II were of 41.0 +/- A 9.6\%,
   40.5 +/- A 9.9\% and 21.3 +/- A 7.4 g, respectively, for octopuses fed
   squid, hake or crayfish, and were always higher for octopuses fed squid
   and hake compared to crayfish. The results indicate that crayfish is not
   an adequate replacement for the usual prey to fatten octopus, even
   considering its much lower market price.},
