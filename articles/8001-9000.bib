@article{ WOS:000232667000006,
Author = {Zúñiga, B and Guevara-Fefer, P and Herrera, J and Contreras, JL and
   Velasco, L and Pérez, FJ and Esquivel, B},
Title = {Chemical composition and anti-inflammatory activity of the volatile
   fractions from the bark of eight Mexican Bursera species},
Journal = {PLANTA MEDICA},
Year = {2005},
Volume = {71},
Number = {9},
Pages = {825-828},
Month = {SEP},
Abstract = {The volatile fractions from the bark of eight species of Mexican Bursera
   were obtained using steam distillation and were subjected to tandem
   GC-MS analysis for identification of the main constituents. The most
   abundant components of steam volatiles were monoterpenoids from which
   a-terpineol, terpinen-4-ol, alpha-thujene, linalool and limonene were
   most frequently isolated. A series of sesquiterpenes and long-chain
   hydrocarbons were isolated and identified from the barks of some of the
   studied species. Some volatiles were assayed for anti -inflammatory
   activity using the TPA-induced ear edema bioassay in mice. The volatile
   fraction from Bursera lancifolia was about half as active as
   indomethacin.},
