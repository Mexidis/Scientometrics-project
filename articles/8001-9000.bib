@inproceedings{ WOS:000264672000042,
Author = {Morales, I. and Mendoza-Temis, J. and Barea, J. and Hirsch, J. G. and
   Lopez Vieyra, J. C. and Van Isacker, P. and Velazquez, V. and Frank, A.},
Editor = {Blazhev, A and Jolie, J and Warr, N and Zilges, A},
Title = {Robust Calculation of Nuclear Masses by means of Image Reconstruction},
Booktitle = {CAPTURE GAMMA-RAY SPECTROSCOPY AND RELATED TOPICS},
Series = {AIP Conference Proceedings},
Year = {2009},
Volume = {1090},
Pages = {246+},
Note = {13th International Symposium on Capture Gamma-Ray Spectroscopy and
   Related Topics, Univ Cologne, Inst Nucl Phys, Cologne, GERMANY, AUG
   25-29, 2008},
Abstract = {We use a new approach based on image reconstruction techniques to
   improve model-based predictions of nuclear masses. A partial view image
   pattern is built by taking the differences between experimentally
   measured masses and the predictions given by different theoretical
   models which describe the smooth macroscopic behavior. We show that it
   is possible to significantly improve predictions in regions not too far
