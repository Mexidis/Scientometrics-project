Editor = {Ayala, A and CalcaneoRoldan, C and MoralesRuiz, B and PerezLorenzana, A and Raya, A and Sanchez, A and TejadaYeomans, ME},
Title = {Perturbative Quantum Analysis and Classical Limit of the Electron
   Scattering by a Solenoidal Magnetic Field},
Booktitle = {PARTICLES AND FIELDS},
Series = {AIP Conference Proceedings},
Year = {2009},
Volume = {1116},
