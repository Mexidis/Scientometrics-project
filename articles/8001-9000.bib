EISSN = {2007-8706},
Keywords = {frugivory; Artibeus lituratus; Carollia perspicillata; Sturnira lilium},
Keywords-Plus = {SEED DISPERSAL; INDICATORS; ABUNDANCE; HABITATS; CECROPIA},
Research-Areas = {Biodiversity \& Conservation},
