@article{ WOS:000237181300003,
Author = {Villavicencio, M and Roa-Neri, JAE and Jiménez, JL},
Title = {The role of a specific response of media in the Vavilov-Cherenkov effect},
Journal = {FOUNDATIONS OF PHYSICS LETTERS},
Year = {2006},
Volume = {19},
Number = {3},
Pages = {249-258},
Month = {JUN},
Abstract = {It is shown that the condition upsilon > upsilon(p) (omega), which is
   necessary in order to trigger the Vavilov-Cherenkov effect, determines
   the interval of radiated frequencies and it alone cannot establish a
   strong restriction to the velocity of the particle. It is exhibited that
   it is possible to define a general lower bound for the velocity of the
   particle, which does not depend on the frequency when both, a specific
   response of the medium is taken into account and the mentioned condition
   is considered. The minimum value of the phase velocity of light in the
   medium determines the existence of such general lower bound.},
