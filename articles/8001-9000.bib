@article{ WOS:000261215600097,
Author = {Gonzalez-Magana, Olmo and Cabrera-Trujillo, R. and Hinojosa, Guillermo},
Title = {Collision-induced fragmentation cross sections of CO2},
Journal = {PHYSICAL REVIEW A},
Year = {2008},
Volume = {78},
Number = {5},
Month = {NOV},
Abstract = {A combined experimental and theoretical study of the collision-induced
   dissociation of CO2+ ions colliding with He in the energy range of 1 to
   9 keV has been carried out in this work. Total cross sections for the
   dissociation products O+, C+, and CO+ are presented as a mean to
   investigate the collision dynamics of this molecular ion. A remarkable
   change on the dissociation dynamics is resolved at about similar to 4
   keV collision energy. From the comparison of the measured cross sections
   to a theoretical treatment of the collision process using
   electron-nuclear dynamics, we infer that electronic multireference
   configurations are required in the dynamics at low collision energies.
   For the C+ dissociation product, the collision dynamics is fully
   described by theory with the possible isomerization of the CO2+ being
   discarded at these collision energies. As a consequence of the
   calibration procedure in the experimental method, double electron
   capture cross section for protons on argon are also reported in good
   agreement with other available experiments.},
