@article{ WOS:000253822100007,
Author = {Vega, J. J. and Reynoso, R. and Calvet, H. Carrillo},
Title = {Learning limits of an artificial neural network},
Journal = {REVISTA MEXICANA DE FISICA},
Year = {2008},
Volume = {54},
Number = {1, S},
