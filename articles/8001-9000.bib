ISSN = {0716-078X},
EISSN = {0717-6317},
Keywords = {panbiogeography; track analysis; Andes; Adesmia; PAE},
Keywords-Plus = {HISTORICAL BIOGEOGRAPHY; GENERALIZED TRACKS; NEW-ZEALAND;
   PANBIOGEOGRAPHY},
