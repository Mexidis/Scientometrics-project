@incollection{ WOS:000286536700001,
Author = {Morrone, Juan J.},
Editor = {Gailis, M and Kalnins, S},
Title = {EVOLUTIONARY BIOGEOGRAPHY: PRINCIPLES AND METHODS},
Booktitle = {BIOGEOGRAPHY},
Series = {Environmental Research Advances},
Year = {2010},
Pages = {1-62},
Abstract = {Evolutionary biogeography integrates distributional, phylogenetic,
   molecular and paleontological data in order to discover biogeographic
   patterns and assess the historical changes that have shaped them,
   following a step-wise approach. Although most of the authors involved in
   the theoretical development of biogeography or that apply specific
   methods usually conceive them as representing alternative ``schools{''},
   they can be used to answer different questions. This review introduces
   the steps of this integrative approach and details the available
   methods. An evolutionary biogeographical analysis involves five steps:
   (1) recognition of biotic components (sets of spatio-temporally
   integrated taxa due to common history), through panbiogeography and
   methods used to identify areas of endemism; (2) contrastation of the
   biotic components and identification of the vicariant events that
   fragmented them, through cladistic biogeography and comparative
   phylogeography; (3) establishment of a hierarchic arrangement of the
   components in a biogeographic system of realms, regions, dominions,
   provinces and districts; (4) identification of cenocrons (sets of taxa
   with similar origins and ages), dated using intraspecific
   phylogeography, molecular clocks and fossils; and (5) formulation of a
   geobiotic scenario, that explains the evolution of the biotic components
   and cenocrons, integrating geological and tectonical information.},
