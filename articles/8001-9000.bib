@article{ WOS:000247001500013,
Author = {Nadler, Jr., Sam B. and Pellicer-Covarrubias, Patricia},
Title = {Cones that are 1/2-homogeneous},
Journal = {HOUSTON JOURNAL OF MATHEMATICS},
Year = {2007},
Volume = {33},
Number = {1},
Pages = {229-247},
Abstract = {A space is 1/2-homogeneous provided that there are exactly two orbits
   for the action of 2 the group of homeomorphisms of the space onto
   itself. Let X be a nonempty compact metric space such that the cone over
   X is 1/2-homogeneous. It is shown that if X is finite-dimensional, then
   X is an absolute neighborhood retract. A general theorem is proved which
   shows that finite dimensionality is necessary. It is shown that if X is
   a 1-dimensional continuum or if X does not contain certain types of
   triods in some nonempty open set, then X is an arc or a simple closed
   curve (assuming Cone(X) is 1/2-homogeneous). A number of corollaries are
   derived. Some unanswered questions are stated.},
