@article{ WOS:000268480000006,
Author = {Larralde, Hernan and Sanders, David P.},
Title = {Long-range correlations in a simple stochastic model of coupled
   transport},
Journal = {JOURNAL OF PHYSICS A-MATHEMATICAL AND THEORETICAL},
Year = {2009},
Volume = {42},
Number = {33},
Month = {AUG 21},
Abstract = {We study coupled transport in the nonequilibrium stationary state of a
   model consisting of independent random walkers, moving along a
   one-dimensional channel, which carry a conserved energy-like quantity,
   with density and temperature gradients imposed by reservoirs at the ends
   of the channel. In our model, walkers interact with other walkers at the
   same site by sharing energy at each time step, but the amount of energy
   carried does not affect the motion of the walkers. We find that already
   in this simple model long-range correlations arise in the nonequilibrium
   stationary state which are similar to those observed in more realistic
   models of coupled transport. We derive an analytical expression for the
   source of these correlations, which we use to obtain semi-analytical
   results for the correlations themselves assuming a local-equilibrium
   hypothesis. These are in very good agreement with results from direct
   numerical simulations.},
