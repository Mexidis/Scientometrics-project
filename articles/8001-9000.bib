@article{ WOS:000260985500018,
Author = {Bortolini, Jose Luis and Alvarez, Fernando},
Title = {Hepatopancreas alteration of the blue crab Callinectes sapidus by
   the rhizocephalan barnacle Loxothylacus texanus},
Journal = {JOURNAL OF INVERTEBRATE PATHOLOGY},
Year = {2008},
Volume = {99},
Number = {3},
Pages = {354-356},
Month = {NOV},
Abstract = {A histological study of the hepatopancreas of the blue crab Callinectes
   sapidus parasitized by the rhizocephalan barnacle Loxothylacus texanus
   was conducted to explore if the degree of development of the parasite's
   rootlet system was correlated to its maturation process as seen by
   external characters of its reproductive body or externa. Four types of
   crabs were examined: control, with virgin and mature externa, and
   scarred. A clear progression with an increase in number and size of the
   parasite's rootlets in the hosts' hepatopancreas can be seen. Although
   the hepatopancreatic tubules remain functional, the hepatopancreas
   appears as a loose structure, completely infiltrated with L. texanus
   rootlets, in advanced stages of the parasitism. (C) 2008 Elsevier Inc.
   All rights reserved.},
