@article{ WOS:000270440200004,
Author = {Orlov, V. G. and Voitsekhovich, V. V. and Mendoza-Valencia, G. A. and
   Svyryd, A. and Rivera, J. L. and Ortiz, F. and Guerrero, C. A.},
Title = {SPECKLE INTERFEROMETRY AT THE OBSERVATORIO ASTRONOMICO NACIONAL. I},
Journal = {REVISTA MEXICANA DE ASTRONOMIA Y ASTROFISICA},
Year = {2009},
Volume = {45},
Number = {2},
Pages = {155-159},
Month = {OCT},
Abstract = {We present the results of speckle interferometric measurements of binary
   stars performed in November, 2008 with the I m telescope at the
   Observatorio Astronomico Nacional of Tonanzintla (Puebla, Mexico). The
   data include 175 position angle and separations measured for 163
   systems. The measured angular separations range from 0.15 `' to 4.00 `'.
   The maximum magnitude of the brighter components is equal to 9.32. The
