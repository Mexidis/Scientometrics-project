@article{ WOS:000183444300009,
Author = {Castaño-Meneses, G and Palacios-Vargas, JG},
Title = {Effects of fire and agricultural practices on neotropical ant
   communities},
Journal = {BIODIVERSITY AND CONSERVATION},
Year = {2003},
Volume = {12},
Number = {9},
Pages = {1913-1919},
Month = {SEP},
Abstract = {Fire is extensively used in agricultural management in Mexico. There is
   little information on the effects of those practices on the abundance
   and diversity of animals that live within these forest soils. We studied
   the effect of slashing, burning and land use in a tropical deciduous
   forest on ant communities in the State of Jalisco, Mexico. The original
   vegetation (tropical deciduous forest) was modified into a corn field.
   Sampling was carried out in five stages: before slashing, after
   slashing, after burning, after seeding and after harvest. We found that
   very severe fires greatly reduced ant diversity. The most important
   effect of fire was the reduction of ant density, and the change of
   species composition and trophic guilds. These changes are relevant in
   the recycling process of energy in the ecosystem.},
