@article{ WOS:000182233100002,
Author = {Hernández, R and Rodríguez, A and Salgado, P and Barrios, FA},
Title = {Concentric dual-loop RF coil for magnetic resonance imaging},
Journal = {REVISTA MEXICANA DE FISICA},
Year = {2003},
Volume = {49},
