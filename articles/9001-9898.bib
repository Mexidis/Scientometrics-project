Journal = {COMPARATIVE PARASITOLOGY},
Year = {2003},
Volume = {70},
Number = {1},
Pages = {11-25},
Month = {JAN},
Abstract = {Ninety-eight Mexican free-tailed bats, Tadarida brasiliensis mexicana,
   collected from 4 locations in Mexico between November 1996 and June
   1998, were examined for metazoan parasites. Twenty-one parasitic taxa
