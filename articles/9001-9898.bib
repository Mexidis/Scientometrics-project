@article{ WOS:000167623900016,
Author = {Berrones, A and Larralde, H},
Title = {Simple model of a random walk with arbitrarily long memory - art. no.
   031109},
Journal = {PHYSICAL REVIEW E},
Year = {2001},
Volume = {63},
Number = {3, 1},
Month = {MAR},
Abstract = {We present a generalization of the persistent random-walk model in which
   the step at time n depends on the stare of the step at time n-T, for
   arbitrary T. This gives rise to arbitrarily long memory effects, yet by
   an appropriate transformation the model is tractable by essentially the
   same techniques applicable to the usual persistent random-walk problem.
