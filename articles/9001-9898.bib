@article{ WOS:A1983RW48200008,
Author = {MURILLO, RM and PALACIOS, JG and LABOUGLE, JM and HENTSCHEL, EM and
   LLORENTE, JE and LUNA, K and ROJAS, P and ZAMUDIO, S},
Title = {SEASONAL-VARIATION OF THE ENTOMOLOGICAL FAUNA ASSOCIATED WITH TILLANDSIA
   SPP IN A ZONE OF BIOTIC TRANSITION},
Journal = {SOUTHWESTERN ENTOMOLOGIST},
Year = {1983},
Volume = {8},
Number = {4},
Pages = {292-298},
Abstract = {The entomological fauna associated with Tillandsia in the Derrame
   Volcanico del Chichinautzin {[}Mexico] belongs to 12 different orders:
   Coleoptera, Collembola, Diptera, Dermaptera, Dictyoptera, Heteroptera,
   Homoptera, Hymenoptera, Orthoptera, Psocoptera, Lepidoptera and
   Thysanoptera. The more important families, for their number of role as
   indicators of seasonal variation, were: Pentatomidae, Lygaeidae,
   Chrysomelidae, Carabidae, Staphylinidae, Reduviidae and Vespidae, of
   these, monthly density variation graphs are presented. The changes in
   density and composition of the entomological fauna, correlated with the
   enviromental conditions, led to the conclusion that the tilandsias are
   only a refuge or shelter and to refute the hypothesis that epiphytic
   bromeliads and associated fauna are a true ecosystem.},
