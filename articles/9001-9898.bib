@article{ WOS:A1993NY99100066,
Author = {BORAU, J and GONZALEZ, A and ESPINOSA, G and GOLZARRI, JI},
Title = {MEASUREMENTS OF RADON LEVELS INSIDE MEXICAN CAVES},
Journal = {NUCLEAR TRACKS AND RADIATION MEASUREMENTS},
Year = {1993},
Volume = {22},
Number = {1-4},
Pages = {287-288},
Abstract = {Living animal species on earth have been exposed to environmental radon
   from the very beginning of time. The effects of radiation, combined with
   other natural parameters such as temperature, humidity, salt contents,
   etc., have most likely influenced the evaluation of different species.
