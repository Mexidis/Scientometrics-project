@article{ WOS:A1997XZ65400005,
Author = {LeonAlvarez, D and ServiereZaragoza, E and GonzalezGonzalez, J},
Title = {Description of the tetrasporangial crustose and gametangial erect phases
   of Ahnfeltiopsis gigartinoides (J. Ag.) Silva et DeCew (Rhodophyta,
   Phyllophoraceae) in Bahia de Banderas, Mexico},
Journal = {BOTANICA MARINA},
Year = {1997},
Volume = {40},
Number = {5},
Pages = {397-404},
Month = {SEP},
Abstract = {Tetrasporophyte and upright gametophyte thalli of a species collected in
   the region of Bahia de Banderas, Mexico are described. Both phases
   coincide with those observed in the life history of the genus
   Ahnfeltiopsis Silva et DeCew (Phyllophoraceae, Rhodophyceae). Crustose
   thalli are not identifiable as any of the red crustose non-coralline
   algae previously described, although they are similar to Erythrodermis
   Batters. The problems of identification of the specimens collected,
   which bear a resemblance to three species reported in Mexico:
   Ahnfeltiopsis concinna (J. Ag.) Silva et DeCew, Ahnfeltiopsis
   gigartinoides (J. Ag.) Silva et DeCew and Ahnfeltia svensonii Taylor,
   are discussed. It is concluded that the plants investigated should be
   assigned to Ahnfelfiopsis gigartinoides and that this is the only
   species of the genus present in the Mexican tropical Pacific.},
