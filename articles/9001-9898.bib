Language = {English},
Affiliation = {Luisi, PL (Corresponding Author), Swiss Fed Inst Technol, Inst Polymers, Univ Str 6, CH-8092 Zurich, Switzerland.
   Swiss Fed Inst Technol, Inst Polymers, CH-8092 Zurich, Switzerland.
   Univ Nacl Autonoma Mexico, Fac Ciencias, Mexico City 04510, DF, Mexico.},
DOI = {10.1002/1522-2675(200206)85:6<1759::AID-HLCA1759>3.0.CO;2-7},
