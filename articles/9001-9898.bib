@article{ WOS:A1994ND45800001,
Author = {CORDOVA, J and TREVINO, C},
Title = {EFFECTS OF LONGITUDINAL HEAT-CONDUCTION OF A VERTICAL THIN-PLATE IN A
   NATURAL CONVECTIVE COOLING PROCESS},
Journal = {WARME UND STOFFUBERTRAGUNG-THERMO AND FLUID DYNAMICS},
Year = {1994},
Volume = {29},
Number = {4},
Pages = {195-204},
Month = {MAR},
Abstract = {This paper analyzes the cooling process of a vertical thin plate caused
   by a free convective flow, taking into account the effects of both
   longitudinal and transversal heat conduction in the plate. Due to the
   finite thermal conductivity of the plate, a longitudinal temperature
   gradient arises within it, which prevents any similarity solution in the
   boundary layer, changing the mathematical character of the problem from
   parabolic to elliptic, for large values of the Rayleigh number. The
   energy balance equations are reduced to a system of three differential
   equations with two parameters: the Prandtl number and a non-dimensional
   plate thermal conductivity alpha. In order to obtain the evolution of
   the temperature of the plate as a function of time and position, the
   coupled balance equations are integrated numerically for several values
   of the parameters, including the cases of very good and poor conducting
   plates. The results obtained, are compared with an asymptotic analysis
   based on the multiple scales technique carried out for the case of a
   very good conducting plate. There is at the beginning a fast transient
   in non-dimensional time scale of order alpha-1 followed by a slow
   non-dimensional time scale of order unity, which gives the evolution of
   the cooling process. Good agreement is achieved even for values of the
   conduction parameter alpha of order unity. The asymptotic solution
   allows us to give closed form analytical solution for the plate
   temperature evolution in time and space. The overall thermal energy of
   the plate decreases faster for smaller values of alpha.},
Publisher = {SPRINGER},
Address = {ONE NEW YORK PLAZA, SUITE 4600, NEW YORK, NY, UNITED STATES},
Type = {Article},
Language = {English},
Affiliation = {CORDOVA, J (Corresponding Author), Univ Nacl Autonoma Mexico, FAC CIENCIAS, DEPT FIS, MEXICO CITY 04510, DF, MEXICO.},
DOI = {10.1007/BF01539750},
