@article{ WOS:000089762200001,
Author = {Bonilla-Barbosa, J and Novelo, A and Orozco, YH and Márquez-Guzmán, J},
Title = {Comparative seed morphology of Mexican Nymphaea species},
Journal = {AQUATIC BOTANY},
Year = {2000},
Volume = {68},
Number = {3},
Pages = {189-204},
Month = {NOV},
Abstract = {Seed morphology was studied in 10 species belonging to three subgenera
   of Nymphaea occurring in Mexico: subgenus Brachyceras (N. ampla, N.
   elegans, N. gracilis, N. pulchella); subgenus Hydrocallis (N. amazonum,
   N. conardii, N. jamesoniana, N. novogranatensis); and subgenus Nymphaea
   (N, mexicana, N. odorata). Phenetic relationships of Mexican species of
   Nymphaea were studied by cluster analysis (UPGMA). Among subgenera, the
   seeds showed morphological variation in shape, color, size,
   ornamentation of sclereids, and trichomes. A cluster analysis
   distinguished the three subgenera of Nymphaea in Mexico. (C) 2000
