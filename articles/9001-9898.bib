@inproceedings{ WOS:000075925600027,
Author = {Okunev, O and Tkacenko, M},
Editor = {Dikranjan, D and Salce, L},
Title = {On thin generating sets in topological groups},
Booktitle = {ABELIAN GROUPS, MODULE THEORY, AND TOPOLOGY: PROCEEDINGS IN HONOUR OF
   ADALBERTO ORSATTI'S 60TH BIRTHDAY},
Series = {LECTURE NOTES IN PURE AND APPLIED MATHEMATICS},
Year = {1998},
Volume = {201},
Pages = {327-342},
Note = {International Conference to Celebrate the 60th Birthday of Adalberto
   Orsatti, ACCADEMIA PATAVINA SCI LETTERE ARTI, PADUA, ITALY, 1997},
Organization = {MURST; CNR; Univ Padova, Dept Math; Univ Ferrara, Dept Math},
Abstract = {If a discrete subset S of a topological group G with identity 1
   generates a dense subgroup of G and S boolean OR \{1\} is closed in G,
   then S is called a suitable set for G. We show that every almost
   metrizable topological group has a suitable set, thus generalizing a
   similar result for metrizable groups obtained by Comfort, Morris,
   Robbie, Svetlichny and Tkacenko. It is also proved that the free
   topological groups of compact spaces from a class including all dyadic
   and all polyadic spaces have suitable sets.},
