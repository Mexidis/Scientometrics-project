@article{ WOS:000188387800009,
Author = {Escalante, T and Espinosa, D and Morrone, JJ},
Title = {Using parsimony analysis of endemicity to analyze the distribution of
   Mexican land mammals},
Journal = {SOUTHWESTERN NATURALIST},
Year = {2003},
Volume = {48},
Number = {4},
Pages = {563-578},
Month = {DEC},
Abstract = {Patterns of endemism of terrestrial mammals inhabiting Mexico are
   analyzed by applying Parsimony Analysis of Endemicity (PAE) to 47
   ecoregions. The ecoregions were classified in 2 main groups: ecoregions
   of the Baja California Peninsula and ecoregions of the rest of the
   country. We found 27 ecoregions with characteristic taxa, and 5 areas of
   endemism: Northern High Plateau, Baja California, Chiapas, Isthmus, and
   Yucatan Peninsula. Groups of ecoregions arc basically congruent with
   other regionalization schemes previously proposed for the country.},
