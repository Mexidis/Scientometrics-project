@article{ WOS:000311583100010,
Author = {Macias, Sergio and Pellicer-Covarrubias, Patricia},
Title = {1/2-HOMOGENEOUS HYPERSPACE SUSPENSIONS},
Journal = {COLLOQUIUM MATHEMATICUM},
Year = {2012},
Volume = {128},
Number = {1},
Pages = {109-132},
Abstract = {We continue the study of 1/2-homogeneity of the hyperspace suspension of
   continua. We prove that if X is a decomposable continuum and its
   hyperspace suspension is 1/2-homogeneous, then X must be continuum
   chainable. We also characterize 1/2-homogeneity of the hyperspace
   suspension for several classes of continua, including: continua
   containing a free arc, atriodic and decomposable continua, and
   decomposable irreducible continua about a finite set.},
Publisher = {ARS POLONA-RUCH},
Address = {KRAKOWSKIE PRZEDMIESCIE 7 PO BOX 1001, 00-068 WARSAW, POLAND},
Type = {Article},
