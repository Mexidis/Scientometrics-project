@article{ WOS:000282111400013,
Author = {Perez-Ramos, Edmundo and Nieto-Montes de Oca, Adrian and
   Vargas-Contreras, Jorge A. and Cordes, James E. and Paulissen, Mark A.
   and Walker, James M.},
Title = {ASPIDOSCELIS SEXLINEATA (SAURIA: TEIIDAE) IN MEXICO:
   DISTRIBUTION, HABITAT, MORPHOLOGY, AND TAXONOMY},
Journal = {SOUTHWESTERN NATURALIST},
Year = {2010},
Volume = {55},
Number = {3},
Pages = {419-425},
Month = {SEP},
Abstract = {We report on Aspidoscelis sexlineata (six-lined racerunner) from the but
   known sites of occurrence in Mexico, one being a southward range
   extension of 74.3 km For the species. These sites are situated in the
   extreme eastern part of the municipality of Matamoros, Tamaulipas, and
   are nearest to records for the species in the United States for Brazos
   Island-South Padre Island, Cameron County, Texas. In both regions, A.
   sexlineata occurs a short distance from the seashore in habitats formed
   and influenced by perturbations of hurricanes and the maritime climate
   of the Gulf of Mexico (e.g., sand dunes devoid of congeners with
   openings in halophytic vegetation). Samples of A. sexlineata from
   Tamaulipas are statistically similar to those from South Padre Island in
   two meristic characters, one ratio, and snout-vent length, but
   significantly different in one meristic character and one ratio
   involving color pattern. Because similarities among specimens from the
   plains of southern Texas, South Padre Island, and Tamaulipas are
   strongly indicated, it is reasonable to allocate the three groups to A.
   sexlineata stephensae. Texas yellow-headed racerunner.},
Publisher = {SOUTHWESTERN ASSOC NATURALISTS},
Address = {SOUTHWEST TEXAS STATE UNIV, DEPT BIOLOGY, 601 UNIVERSITY DR, SAN MARCOS,
   TX 78666 USA},
Type = {Article},
Language = {English},
Affiliation = {Walker, JM (Corresponding Author), Univ Arkansas, Dept Biol Sci, Fayetteville, AR 72701 USA.
