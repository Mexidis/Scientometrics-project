@article{ WOS:000313549100002,
Author = {Jaime, Luisa G. and Patino, Leonardo and Salgado, Marcelo},
Title = {About matter and dark-energy domination eras in Rn
   gravity or lack thereof},
Journal = {PHYSICAL REVIEW D},
Year = {2013},
Volume = {87},
Number = {2},
Month = {JAN 16},
Abstract = {We provide further numerical evidence which shows that R-n models in
   f(R) metric gravity produce a late time acceleration in the Universe or
   a matter domination era (usually a transient one) but not both. Our
   results confirm the findings of Amendola et al. {[}Phys. Rev. Lett. 98,
   131302 (2007); Phys. Rev. D 75, 083504 (2007); Int. J. Mod. Phys. D 16,
   1555 (2007)], but using a different approach that avoids the mapping to
   scalar-tensor theories of gravity, and therefore, spares us from any
   discussion or debate about frames (Einstein vs Jordan) which are endemic
   in this subject. This class of models has been used extensively in the
   literature as an alternative to dark energy, but should be considered
   ruled out for being inconsistent with the observations. Finally, we
   discuss a caveat in the analysis by Faraoni {[}Phys. Rev. D 83, 124044
   (2011)], which was used to further constrain these models by using a
   chameleon mechanism. DOI: 10.1103/PhysRevD.87.024029},
