@article{ WOS:000312172500006,
Author = {Rojas-Pacheco, A. and Obregon-Quintana, B. and Liebovitch, L. S. and
   Guzman-Vargas, L.},
Title = {Time-delay effects on dynamics of a two-actor conflict model},
Journal = {PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS},
Year = {2013},
Volume = {392},
Number = {3},
Pages = {458-467},
Month = {FEB 1},
Abstract = {We present a study of time-delay effects on a two-actor conflict model
   based on nonlinear differential equations. The state of each actor
   depends on its own state in isolation, its previous state, its inertia
   to change, the positive or negative feedback and a time delay in the
   state of the other actor. We use both theoretical and numerical
   approaches to characterize the evolution of the system for several
   values of time delays. We find that, under particular conditions, a time
   delay leads to the appearance of oscillations in the states of the
   actors. Besides, phase portraits for the trajectories are presented to
   illustrate the evolution of the system for different time delays.
   Finally, we discuss our results in the context of social conflict
   models. (C) 2012 Elsevier B.V. All rights reserved.},
