@article{ WOS:000281437500009,
Author = {de las Mercedes Luna-Reyes, Maria and Llorente-Bousquets, Jorge and
   Luis-Martinez, Armando and Vargas-Fernandez, Isabel},
Title = {Faunistic composition and phenology of butterflies (Rhopalocera:
   Papilionoidea) at Canon de Lobos, Yautepec, Morelos, Mexico},
Journal = {REVISTA MEXICANA DE BIODIVERSIDAD},
Year = {2010},
Volume = {81},
Number = {2},
Pages = {315-342},
Month = {AUG},
Abstract = {In order to know the species of Papilionoidea in the area and to
   describe its temporary distribution, the specimens were monthly captured
   from August of 1983 to December of 1986. 145 species of 4 families of
   Papilionoidea were recognized, and added to the obtained registries of
   literature with a total of 148 species for Canon de Lobos; 21 of them
   are endemic to Mexico. Nymphalidae and Lycaenidae were the richest
   families, while Nymphalidae was the most abundant. Like in other
   tropical areas, the temporary distribution of the Papilionoidea is
   associated closely with the precipitation, reason why there were more
   species and specimens during the rainy season. The most of
   rhopalocerofauna of Canon de Lobos is conformed by scarse and rare
   species that are resident in the area; only 10\% of the species is
   associated exclusively to the dry forest. In comparison with other
   localities of Morelos, Canon de Lobos has one of the greater richness.},
