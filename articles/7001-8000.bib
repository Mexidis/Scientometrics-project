@article{ WOS:000291568100133,
Author = {Martinez-Loran, Erick and Alvarez-Zauco, Edgar and Basiuk, Vladimir A.
   and Basiuk, Elena V. and Bizarro, Monserrat},
Title = {Fullerene Thin Films Functionalized by 1,5-Diaminonaphthalene:
   Preparation and Properties},
Journal = {JOURNAL OF NANOSCIENCE AND NANOTECHNOLOGY},
Year = {2011},
