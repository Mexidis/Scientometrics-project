@article{ WOS:000330162400042,
Author = {Acosta-Gutierrez, Roxana},
Title = {Biodiversity of Siphonaptera in Mexico},
Journal = {REVISTA MEXICANA DE BIODIVERSIDAD},
Year = {2014},
Volume = {85},
Number = {S},
Pages = {S345-S352},
Month = {JAN},
Abstract = {Siphonaptera are insect parasites of endotherm vertebrates, birds and
   mammals, occurring more abundantly in the Order Rodentia. Fleas are
   distributed widely in the world, except in Antartica, and are considered
   cosmopolites. This group of insects is diverse, with 2 575 species
   reported worldwide. Mexico has 172 species that belong to 8 families;
   representing 6.8\% of the world's flea fauna. Ceratophyllidae (74
   species) and Ctenophthalmidae (45 species) are the most abundant
   families in Mexico. Fleas have sanitary importance because they are
   vectors of diseases like plague, typhus and helminthiasis.},
