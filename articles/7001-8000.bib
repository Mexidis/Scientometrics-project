@article{ WOS:000315048100004,
Author = {Vargas, Carlos E. and Velazquez, Victor and Lerma, Sergio},
Title = {Microscopic study of neutron-rich dysprosium isotopes},
Journal = {EUROPEAN PHYSICAL JOURNAL A},
Year = {2013},
Volume = {49},
Number = {1},
Month = {JAN},
Abstract = {Microscopic studies in heavy nuclei are very scarce due to large valence
   spaces involved. This computational problem can be avoided by means of
   the use of symmetry-based models. Ground-state, gamma and beta bands,
   and their B(E2) transition strengths in Dy160-168 isotopes, are studied
   in the framework of the pseudo-SU(3) model which includes the preserving
   symmetry Q . Q term and the symmetry-breaking Nilsson and pairing terms,
   systematically parametrized. Additionally, three rotor-like terms are
   considered, whose free parameters, fixed for all members of the chain,
   are used to fine tune the moment of inertia of rotational bands and the
   band head of gamma and beta bands. The model succesfully describes in a
   systematic way rotational features in these nuclei and allows to
   extrapolate toward the midshell nucleus Dy-170. The results presented
   show that it is possible to study a full chain of isotopes or isotones
   in the region with the present model.},
