@article{ WOS:000313466800022,
Author = {Cajas, J. C. and Trevino, C.},
Title = {Transient heating and entropy generation of a fluid inside a large
   aspect ratio cavity},
Journal = {INTERNATIONAL JOURNAL OF THERMAL SCIENCES},
Year = {2013},
Volume = {64},
Pages = {220-231},
Month = {FEB},
Abstract = {In this work, the transient heating of a fluid inside a vertical cavity
   of large aspect ratio (height/length) was studied numerically by the use
   of the SIMPLE algorithm. The heat sources are two vertical plates
   localized in the side walls of the cavity near the bottom. Calculations
   were performed for a fixed value of the Prandtl number, Pr = 7, aspect
   ratio of 12 and six different Rayleigh numbers between 10(3) and 10(6).
   The temperature and entropy production fields, the non-dimensional heat
   flux on the heated plates (given by the average Nusselt number) have
   been obtained. From a clear dependence on the Rayleigh number, different
   mechanisms of symmetry break and heat transfer in the cavity were found,
   where vortices dynamics play a very important role. A universal behavior
   of the mean values of the overall reduced entropy production rate was
   found, valid after a short initial transient. (C) 2012 Elsevier Masson
   SAS. All rights reserved.},
