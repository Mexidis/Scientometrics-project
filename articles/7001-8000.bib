@article{ WOS:000302443500003,
Author = {Hodson, David J. and Romano, Nicola and Schaeffer, Marie and Fontanaud,
   Pierre and Lafont, Chrystel and Fiordelisio, Tatiana and Mollard,
   Patrice},
Title = {Coordination of calcium signals by pituitary endocrine cells in
   situ},
Journal = {CELL CALCIUM},
Year = {2012},
Volume = {51},
Number = {3-4, SI},
Pages = {222-230},
Month = {MAR-APR},
Abstract = {The pulsatile secretion of hormones from the mammalian pituitary gland
   drives a wide range of homeostatic responses by dynamically altering the
   functional set-point of effector tissues. To accomplish this, endocrine
   cell populations residing within the intact pituitary display
   large-scale changes in coordinated calcium-spiking activity in response
   to various hypothalamic and peripheral inputs. Although the pituitary
   gland is structurally compartmentalized into specific and intermingled
   endocrine cell networks, providing a clear morphological basis for such
   coordinated activity, the mechanisms which facilitate the timely
   propagation of information between cells in situ remain largely
   unexplored. Therefore, the aim of the current review is to highlight the
   range of signalling modalities known to be employed by endocrine cells
   to coordinate intracellular calcium rises, and discuss how these
   mechanisms are integrated at the population level to orchestrate cell
   function and tissue output. (C) 2011 Elsevier Ltd. All rights reserved.},
