@article{ WOS:000312071300013,
Author = {Lopez-Monsalvo, C. S. and Nettel, F. and Sanchez, A.},
Title = {Comment on ``Geometrothermodynamics of a Charged Black Hole of String
   Theory{''}},
Journal = {BRAZILIAN JOURNAL OF PHYSICS},
Year = {2012},
Volume = {42},
Number = {5-6},
Pages = {422-424},
Month = {DEC},
Abstract = {We comment on the conclusions found by Larraaga and Mojica (Braz J Phys
   41:154, 2011) regarding the consistency of the geometrothermodynamics
   programme to describe the critical behaviour of a
   Gibbons-Maeda-Garfinkle-Horowitz-Strominger charged black hole. We argue
   that making the appropriate choice of metric for the thermodynamic phase
   space and, most importantly, considering the homogeneity of the
   thermodynamic potential we obtain consistent results for such a black
   hole.},
Publisher = {SPRINGER},
Address = {233 SPRING ST, NEW YORK, NY 10013 USA},
