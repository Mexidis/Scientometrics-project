   Bernede, J. C.},
Editor = {Shaheen, SE and Olson, DC and Dennler, G and Mozer, AJ and Kroon, JM},
Title = {The influence of the HOMO energy level of the organic electron donor on
   the effectiveness of the anode buffer layer in photovoltaic solar cells},
Booktitle = {PROCEEDINGS OF THE SPRING 2011 E-MRS MEETING, SYMPOSIUM S: ORGANIC
   PHOTOVOLTAICS: SCIENCE AND TECHNOLOGY},
