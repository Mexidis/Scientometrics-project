@article{ WOS:000309487700049,
Author = {Dagnino, E. P. and Chamorro, E. R. and Romano, S. D. and Felissia, F. E.
   and Area, M. C.},
Title = {Optimization of the acid pretreatment of rice hulls to obtain
   fermentable sugars for bioethanol production},
Journal = {INDUSTRIAL CROPS AND PRODUCTS},
Year = {2013},
Volume = {42},
Pages = {363-368},
Month = {MAR},
Abstract = {The structural complexity of the lignocellulosic materials hinders
   enzymatic hydrolysis for what their conversion to bioethanol requires a
   pretreatment step. The aim of this work was to optimize the pretreatment
   of rice hulls with diluted acid. A Central Composite Design (CCD) was
   used to obtain regression equations in function of the variables: acid
   concentration and heating time. Optimal conditions were obtained by the
   Desirability function. As a strategy to obtain the best solid material
   for the production of ethanol by fermentation minimizing sugars
   degradation, the optimization of the pretreatments was performed
   following three scenarios. The optimum was established as the conditions
   that maximize glucans in the solid and xylose in the liquid (0.3\% (w/v)
   of sulfuric acid and a 33 min). The pretreated rice hull in those
   conditions was treated enzymatically. The performance of the enzymatic
   hydrolysis was about 50\% (25\% of total sugars present) over a period
   of 48 h of reaction, and the efficiency of conversion of dissolved
   sugars to bioethanol was of 84\%. (C) 2012 Elsevier B.V. All rights
   reserved.},
