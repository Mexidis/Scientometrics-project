Title = {Tangled in a sparse spider web: single origin of orb weavers and their
   spinning work unravelled by denser taxonomic sampling},
Journal = {PROCEEDINGS OF THE ROYAL SOCIETY B-BIOLOGICAL SCIENCES},
Year = {2012},
