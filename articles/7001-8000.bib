@article{ WOS:000292777600009,
Author = {Villanueva-Almanza, Lorena and Maria Fonseca, Rosa},
Title = {Taxonomic review and geographic distribution of Ephedra
   (Ephedraceae) in Mexico},
Journal = {ACTA BOTANICA MEXICANA},
Year = {2011},
Volume = {96},
Abstract = {This research seeks to determine the number of species of Ephedra in
   Mexico and their distributions in the national territory. Eight species
   of Ephedra are distributed in North and Central Mexico: Ephedra
   antisyphilitica, E. aspera, E. californica, E. compacta, E. nevadensis,
   E. pedunculata, E. torreyana and E. trifurca. Ephedra compacta is the
   only species endemic to Mexico and the one with the most widely
   distribution over the country. A key, descriptions for each of the
   species, and distribution maps are presented. Ephedra species are found
   between 0 and 2800 m above sea level, mostly in xerophytic scrublands,
   grasslands, and thorn forests.},
