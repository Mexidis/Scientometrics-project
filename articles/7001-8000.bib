@article{ WOS:000331339700003,
Author = {Antonyan, Natella and Antonyan, Sergey and Martin-Peinador, Elena},
Title = {Equivariant embeddings of metrizable proper G-spaces},
Journal = {TOPOLOGY AND ITS APPLICATIONS},
Year = {2014},
Volume = {163},
Number = {SI},
Pages = {11-24},
Month = {FEB 15},
Abstract = {For a locally compact group G we consider the class G-M of all proper
   (in the sense of R. Palais) G-spaces that are metrizable by a
   G-invariant metric. We show that each X is an element of G-M admits a
   compatible G-invariant metric whose closed unit balls are small subsets
   of X. This is a key result to prove that X admits a closed equivariant
   embedding into an invariant convex subset V of a Banach G-space L such
   that L\textbackslash{}\{0\} E is an element of G-M and V is a G-absolute
   extensor for the class G-M. On this way we establish two equivariant
   embedding results for proper G-spaces which may be considered as
   equivariant versions of the well-known Kuratowski-Wojdyslawski theorem
   and Arens-Eells theorem, respectively. (C) 2013 Elsevier B.V. All rights
   reserved.},
