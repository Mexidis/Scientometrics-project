@article{ WOS:000326594000001,
Author = {Jimenez, J. L. and Campos, I. and Lopez-Marino, M. A.},
Title = {Several energy-momentum-stress balance equations deduced from Maxwell's
   equations in material media. Non-covariant and explicitly covariant
   formulation},
