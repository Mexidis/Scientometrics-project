@article{ WOS:000297794800014,
Author = {Hernandez, G. and del Valle, G. and Campos, I. and Jimenez, J. L.},
Title = {Motion of a falling drop with accretion using Newtonian methods},
Journal = {REVISTA BRASILEIRA DE ENSINO DE FISICA},
Year = {2011},
Volume = {33},
Number = {3},
Month = {SEP},
Abstract = {The motion of a falling drop whose mass grows by accretion is studied
   with Newtonian methods to the point of finding the position as a
   function of time. The equation of motion applied is the equation of
   motion of continuum mechanics in its Eulerian or space formulation. We
   study three examples of laws of accretion: mass growing linearly with
   time, mass growing linearly with the surface of the drop and mass
   growing proportionally to the product of surface and velocity. These
   examples are sometimes left as exercises, without further discussion,
   asking only for nu(z), or the final velocity. We also show that the
   solutions have the correct limit of a particle of constant mass in free
   fall and of such a particle with friction linear in the velocity.},
