@article{ WOS:000338985600002,
Author = {Rios-Casanova, L. and Castano, G. and Farias-Gonzalez, V. and Davila, P.
   and Godinez-Alvarez, H.},
Title = {Activity Patterns of the Red Harvester Ant in a Mexican Tropical Desert},
Journal = {SOCIOBIOLOGY},
Year = {2014},
Volume = {61},
Number = {2},
Pages = {133-135},
Abstract = {Red harvester ant (Pogonomyrmex barbatus) inhabits deserts of USA and
   Mexico. Its activity patterns are well known in temperate deserts, but
   they have not been studied in tropical ones. We studied these patterns
   in the Tehucan Valley, a tropical desert in central Mexico. It had
   bimodal activity patterns in spring, summer, and fall while unimodal
   patterns in winter. These patterns differ from those reported for this
   species in temperate deserts where activity stopped in winter. Our
   results suggest that P. barbatus extends its activity periods and
   remains active all year round in the Tehuacan Valley.},
