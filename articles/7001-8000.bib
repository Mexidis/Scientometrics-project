   and Tomasi, D. and Rodriguez, A. O.},
Editor = {BernalAlvarado, JD and GuzmanCabrera, R and Brandan, ME},
Title = {Multi Circular-Cavity Surface Coil for Magnetic Resonance Imaging of
   Monkey's Brain at 4 Tesla},
