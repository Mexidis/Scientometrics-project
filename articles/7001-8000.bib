@article{ WOS:000292741800018,
Author = {Sanchez-Sesma, Francisco J. and Rodriguez, Miguel and Iturraran-Viveros,
   Ursula and Luzon, Francisco and Campillo, Michel and Margerin, Ludovic
   and Garcia-Jerez, Antonio and Suarez, Martha and Santoyo, Miguel A. and
   Rodriguez-Castellanos, Alejandro},
Title = {A theory for microtremor H/V spectral ratio: application for a
   layered medium},
Journal = {GEOPHYSICAL JOURNAL INTERNATIONAL},
Year = {2011},
Volume = {186},
Number = {1},
Pages = {221-225},
Month = {JUL},
Abstract = {Microtremors are produced by multiple random sources close to the
   surface of the Earth. They may include the effects of multiple
   scattering, which suggests that their intensities could be well
   described by diffusion-like equations. Within this theoretical
   framework, the average autocorrelation of the motions at a given
   receiver, in the frequency domain, measures average energy density and
   is proportional to the imaginary part of the Green's function (GF) when
   both source and receiver are the same.
   Assuming the seismic field is diffuse we compute the H/V ratio for a
   surface receiver on a horizontally layered medium in terms of the
   imaginary part of the GF at the source. This theory links average energy
   densities with the GF in 3-D and considers the H/V ratio as an intrinsic
   property of the medium. Therefore, our approach naturally allows for the
   inversion of H/V, the well-known Nakamura's ratio including the
   contributions of Rayleigh, Love and body waves. Broad-band noise records
   at Texcoco, a soft soil site near Mexico City, are studied and
   interpreted using this theory.},
