@article{ WOS:000291568100130,
Author = {Basiuk (Golovataya-Dzhymbeeva), Elena V. and Ochoa-Olmos, Omar and
   Contreras-Torres, Flavio F. and Meza-Laguna, Victor and Alvarez-Zauco,
   Edgar and Puente-Lee, Ivan and Basiuk, Vladimir A.},
Title = {``Green{''} Functionalization of Pristine Multi-Walled Carbon Nanotubes
   with Long-Chain Aliphatic Amines},
