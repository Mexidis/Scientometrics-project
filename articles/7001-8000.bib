@article{ WOS:000300459400119,
Author = {Avila, O. and Torres-Ulloa, C. L. and Medina, L. A. and
   Trujillo-Zamudio, F. E. and Gamboa-deBuen, I. and Buenfil, A. E. and
   Brandan, M. E.},
Title = {TL measurement of ambient dose at a Nuclear Medicine Department},
Journal = {RADIATION MEASUREMENTS},
Year = {2011},
Volume = {46},
Number = {12, SI},
Pages = {1843-1846},
Month = {DEC},
Abstract = {An investigation of ambient dose at the Nuclear Medicine Department,
   Institut Nacional de Cancerologia. Mexico, was performed using two types
   of thermoluminescent dosemeters: TLD-100 and TLD-900. In addition,
   ambient dose was measured at the outside corridor of the hospitalization
   room for, (CS)-C-137 brachytherapy patients. Radionuclides used at the
   Nuclear Medicine Department are I-131, F-18, Ga-67, Tc-99m, In-111,
   Tl-201 Main gamma emission energies from these elements are in the range
   from 93 to 511 keV. High dose values were obtained at the waste storage
   room, the outside corridor of I-131 patients, the PET/CT room and the
   outside corridor of Cs-137 brachytherapy patients. The maximum ambient
   dose equivalent rate equal to about 19 mu Sv/h was obtained for the
   Cs-137 brachytherapy corridor. Sites with minimum doses are the gamma
   camera rooms, having ambient dose equivalent rates equal to about 0.05
   mu Sv/h. At most sites where both types of dosemeters were placed
   ambient dose equivalent values obtained with TLD-900 were 25-45\% higher
   than TLD-100 values. This result was attributed to scattered radiation
   producing low energy photons which results in over response of TLD-900
   and thus, TLD-100 values are considered to be more reliable. Knowledge
   of ambient dose values is relevant to the occupational exposure
   personnel who may limit the time spent at high dose areas.
   Recommendations were given to the Department authorities to implement
   actions in order to reduce doses at high dose sites in order to comply
   with the ALARA principle. (C) 2011 Elsevier Ltd. All rights reserved.},
